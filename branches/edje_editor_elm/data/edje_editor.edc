                  font: "Vera";
                  size: 9;
                  fit: 0 0;
                  align: 0.0 0.0;
               }
            }
         }
         /* Resize MiniArrow Area*/
         part{
            name: "miniarrow";
            type: RECT;
            mouse_events: 1;
            description{
               state: "default" 0.0;
               color: 0 0 0 0;
               rel1 {relative: 0 0; offset: 0 0;}
               rel2 {relative: 0 0; offset: 7 7;}
            }
            dragable {
               //confine: "miniarrow";
               //events:  ;
               x: 1 1 1;
               y: 1 1 1;
            }
         }
      }
      programs {
         /* Resize MiniArrow Area*/
         program{
            name: "StartDrag";
            signal: "drag";
            source: "miniarrow";
            action: SIGNAL_EMIT "DRAG" "miniarrow";
         }
         program{
            name: "EndDrag";
            signal: "mouse,up,1";
            source: "miniarrow";
            action: SIGNAL_EMIT "END_DRAG" "miniarrow";
         }
      }
   }
