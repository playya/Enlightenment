
part {
  name: "color.clip";
  type: RECT;
  description {
    state: "default" 0.0;
    color: 255 255 255 0;
    visible:, 0;
    rel1 {
      to: "panel.clip";
    }
    rel2 {
      to: "panel.clip";
    }
  }
  description {
    state: "selected" 0.0;
    color: 255 255 255 255;
    visible: 1;
    rel1 {
      to: "panel.clip";
    }
    rel2 {
      to: "panel.clip";
    }
  }
}

/* layout everything relative to this */
part {
  name: "color.box";
  type: RECT;
  clip_to: "color.clip";
  description {
    state: "default" 0.0;
    color: 255 255 255 0;
    min: 91 63;
    max: 91 63;
    visible, 0;
    rel1 {
      to: "tabs.trough";
      relative: 0 1;
      offset: 0 1;
    }
    rel2 {
      to: "panel.clip";
      relative: 1 1;
      offset: -2 -2;
    }
  }
}


