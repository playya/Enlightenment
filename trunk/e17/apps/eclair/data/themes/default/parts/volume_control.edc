part {
   name: "volume_bar";
   description {
      state: "default" 0.0;
      color: 215 215 215 255;
      rel1 {
         relative: 0 0;
         offset: 192 89;
         to: "body";
      }
      rel2 {
         relative: 0 0;
         offset: 264 90;
         to: "body";
      }
      image {
         normal: "progress_bar.png";
         border: 5 5 0 0;
      }
   }
}
part {
   name: "volume_bar_confine";
   type: RECT;
   mouse_events: 0;
   description {
      state: "default" 0.0;
      visible: 0;
      rel1 {
         relative: 0.0 0.0;
         offset: -8 14;
         to: "volume_bar";
      }
      rel2 {
         relative: 1.0 0.0;
         offset: 8 15;
         to: "volume_bar";
      }
   }
}
part {
   name: "volume_bar_drag";
   dragable {
      confine: "volume_bar_confine";
      x: 1 1 1;
      y: 0 0 0;
   }
   description {
      state: "default" 0.0;
      min: 32 32;
      max: 32 32;
      image {
         normal: "slider_up.png";
      }
   }
   description {
      state: "down" 0.0;
      inherit: "default" 0.0;
      image {
         normal: "slider_down.png";
      }
   }
}
part {
   name: "volume_bar_drag_glow";
   mouse_events: 0;
   description {
      state: "default" 0.0;
      color: 255 255 255 0;
      rel1 {
         to: "volume_bar_drag";
      }
      rel2 {
         to: "volume_bar_drag";
      }
      image {
         normal: "slider_glow.png";
      }
   }
   description {
      state: "visible" 0.0;
      inherit: "default" 0.0;
      color: 255 255 255 255;
   }
}
