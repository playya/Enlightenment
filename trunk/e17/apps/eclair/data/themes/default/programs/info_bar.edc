program {
   name: "info_scroll_left";
	action: STATE_SET "right" 0.0;
	transition: SINUSOIDAL 2.0;
	target: "current_media_name";
	in: 1.5 0.0;
	after: "info_scroll_right";
}
program {
	name: "info_scroll_right";
	action: STATE_SET "default" 0.0;
	transition: SINUSOIDAL 2.0;
	target: "current_media_name";
	in: 1.5 0.0;
	after: "info_scroll_left";
}
