	       state: "out4" 0.0;
	       inherit: "default" 0.0;
	       color: 255 255 255 255;
	       rel1 {
		  relative: 0.5 0.5;
		  offset: -119 -119;
	       }
	       rel2 {
		  relative: 0.5 0.5;
		  offset: 120 120;
	       }
	    }
	 }
	 
	 part {
	    name: "black";
	    mouse_events: 0;
	    type: RECT;
	    description {
	       state: "default" 0.0;
	       color: 0 0 0 0;
	    }
	    description {
	       state: "visible" 0.0;
	       color: 0 0 0 255;
	    }
	 }
      }
      
      programs {
	 program {
	    name: "logo-anim";
	    in: 0.5 0.0;
	    signal: "show";
	    source: "";
	    action: STATE_SET "out" 0.0;
	    target: "logo";
	    target: "logo-shadow1";
	    target: "logo-shadow2";
	    target: "logo-shadow3";
	    transition: LINEAR 0.1;
	    after: "logo-anim2";
	 }
	 program {
	    name: "logo-anim2";
	    action: STATE_SET "out2" 0.0;
	    target: "logo";
	    target: "logo-shadow1";
	    target: "logo-shadow2";
	    target: "logo-shadow3";
	    transition: LINEAR 0.15;
	    after: "logo-anim3";
	 }
	 program {
	    name: "logo-anim3";
	    action: STATE_SET "out3" 0.0;
	    target: "logo";
	    target: "logo-shadow1";
	    target: "logo-shadow2";
	    target: "logo-shadow3";
	    transition: LINEAR 0.2;
	    after: "logo-anim4";
	 }
	 program {
	    name: "logo-anim4";
	    action: STATE_SET "out4" 0.0;
	    target: "logo";
	    target: "logo-shadow1";
	    target: "logo-shadow2";
	    target: "logo-shadow3";
	    transition: LINEAR 0.25;
	 }
	 
	 program {
	    name: "message";
	    signal: "exquisite";
	    source: "message";
	    action: STATE_SET "default" 0.0;
	    target: "exquisite.message";
	    after: "message2";
	 }
	 program {
	    name: "message2";
	    action: STATE_SET "visible" 0.0;
	    target: "exquisite.message";
	    transition: LINEAR 0.25;
	 }
	 
	 program {
	    name: "title";
	    signal: "exquisite";
	    source: "title";
	    action: STATE_SET "default" 0.0;
	    target: "exquisite.title";
	    after: "title2";
	 }
	 program {
	    name: "title2";
	    action: STATE_SET "visible" 0.0;
	    target: "exquisite.title";
	    transition: LINEAR 0.25;
	 }

	 program {
	    name: "tick";
	    signal: "exquisite";
	    source: "tick";
	    action: STATE_SET "default" 0.0;
	    target: "ball";
	    after: "tick2";
	 }
	 program {
	    name: "tick2";
	    action: STATE_SET "visible" 0.0;
	    target: "ball";
	    transition: LINEAR 0.5;
	 }
	 
	 program {
	    name: "exit";
	    signal: "exquisite";
	    source: "exit";
	    action: STATE_SET "visible" 0.0;
	    target: "black";
	    transition: LINEAR 0.5;
	    after: "exit2";
	 }
	 program {
	    name: "exit2";
	    action: SIGNAL_EMIT "exquisite" "done";
	 }
	 
	 program {
	    name: "pulsate";
	    signal: "exquisite";
	    source: "pulsate";
	    action: STATE_SET "pulse" 1.0;
	    target: "bar";
	    after: "pulsate2";
	 }
	 program {
	    name: "pulsate2";
	    action: STATE_SET "pulse" 0.0;
	    target: "bar";
	    transition: SINUSOIDAL 0.8;
	    after: "pulsate3";
	 }
	 program {
	    name: "pulsate3";
	    action: STATE_SET "pulse" 1.0;
	    target: "bar";
	    transition: SINUSOIDAL 0.8;
	    after: "pulsate2";
	 }
	 
	 program {
	    name: "pulsate_stop";
	    action: ACTION_STOP;
	    target: "pulsate2";
	    target: "pulsate3";
	 }
	 
	 program {
	    name: "progress";
	    signal: "exquisite";
	    source: "progress";
	    action: STATE_SET "default" 0.0;
	    target: "bar";
	    after: "pulsate_stop";
	 }
      }
   }
}
