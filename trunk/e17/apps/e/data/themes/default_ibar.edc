      program {
	 name:    "exec_exec";
	 signal:  "exec";
	 source:  "";
	 action:  STATE_SET "faded" 0.0;
         transition: LINEAR 0.5;
	 target:  "item_clip";
	 after:   "exec_stop";
      }
      program {
	 name:    "exec_stop";
	 signal:  "stop";
	 source:  "";
	 action:  STATE_SET "default" 0.0;
	 target:  "item_clip";
      }
   }
}

