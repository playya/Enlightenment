#!/bin/sh

display_help() {
  echo "usage: e_util_eapp_edit [OPTIONS] file.eap"
  echo "         if <file.eap> doesn't exist a new file will be created"
  echo ""
  echo " [OPTIONS]"
  echo "  -h             - view this help screen."
#  echo "  -l [str]       - Set the language for the meta data."
#  echo "  -c [str]       - Set the window class."
}

if [ ! $1 ]; then
  display_help
  exit;
fi

case "$1" in
  "-h" | "--help" )
    display_help
    exit
  ;;
  *)
    enlightenment_remote -start-eap-edit $*
  ;;
esac

