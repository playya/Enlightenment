		target, "session_list_confine";
		transition, LINEAR 0.95;
	    }
	    /* ani in */
	    program {
		name, "slide_in_session_list";
		signal, "session,list,in";
		source, "";
		script {
		    if(get_int(session_list_visible) == 0)
		    {
			set_int(session_list_visible, 1);
			anim(1.0, "animate_session_list_in", 5);
			run_program(PROGRAM:"fade_in_session_list");
		    }
		    else
		    {
			emit("BAR", "QUIT");
		    }
		}
	    }
