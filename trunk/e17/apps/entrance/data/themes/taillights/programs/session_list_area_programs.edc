	    program {
		name, "slide_out_session_list";
		signal, "session,list,out";
		source, "";
		script {
		    if(get_int(session_list_visible) == 1)
		    {
			set_int(session_list_visible, 0);
			anim(1.0, "animate_session_list_out", 5);
			run_program(PROGRAM:"fade_out_session_list");
		    }
		    else
		    {
			emit("FOO", "BAR");
		    }
		}
	    }
