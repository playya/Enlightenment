group {
   name: "notebook";
   data {
      item: "inset/left" "8";
      item: "inset/right" "8";
      item: "inset/bottom" "5";
   }

   parts {
      part {
	 name:          "outline";
	 mouse_events:  0;
	 description {
	    state:    "default" 0.0;
	    rel1 {
	       relative: 0.0  0.0;
	       offset:   3    5;
	    }
	    rel2 {
	       relative: 1.0  1.0;
	       offset:   -3   -3;
	    }
	    image {
	       normal: "e17_frame_outline.png";
	       border: 7 7 7 7;
	       middle: 0;
	    }
	    fill {
               smooth: 0;
	    }
	 }
      }
   }
}

group {
    name, "tab";
    parts {
      part {
	 name:          "label_under";
	 mouse_events:  0;
	 description {
	    state:    "default" 0.0;
	    rel1 {
	       to:       "label_over";
	       relative: 0.0  0.0;
	       offset:   1    1;
	    }
	    rel2 {
	       to:       "label_over";
	       relative: 1.0  1.0;
	       offset:   -2   -2;
	    }
	    image {
	       normal: "e17_frame_label_under.png";
	       border: 3 3 3 3;
	    }
	    fill {
               smooth: 0;
	    }
	 }
	 description {
	    state: "selected" 0.0;
	    inherit, "default" 0.0;
	    image {
		    normal, "e17_frame_label_under_selected.png";
		    border, 3 3 3 3;
	    }
	 }
      }

      part {
	 name:          "label_over";
	 mouse_events:  0;
	 description {
	    state:    "default" 0.0;
	    rel1 {
	       relative: 0.0  0.0;
	       offset:   0   0;
	    }
	    rel2 {
	       relative: 1.0  1.0;
	       offset:   -1    -1;
	    }
	    image {
	       normal: "e17_frame_label_over.png";
	       border: 7 7 7 7;
	    }
	    fill {
               smooth: 0;
	    }
	 }
      }
      programs {
	      program {
		      name, "selected";
		      signal, "selected";
		      source, "EWL";
		      action, STATE_SET "selected" 0.0;
		      target, "label_under";
	      }
	      program {
		      name, "default";
		      signal, "default";
		      source, "EWL";
		      action, STATE_SET "default" 0.0;
		      target, "label_under";
	      }
      }
  }
}
group {
   name: "tab/label";
   data {
      item: "pad/top" "5";
      item: "pad/left" "5";
      item: "pad/right" "5";

      item: "inset/left" "5";
      item: "inset/right" "5";
      item: "inset/top" "3";
      item: "inset/bottom" "0";
   }

   parts {
      part {
          name, "text";
          type, TEXT;
          effect, SOFT_SHADOW;

          description {
	      state, "default" 0.0;
	      align, 0.5 0.5;
	      rel1 {
   	          relative, 0.0 0.0;
	          offset, 0 0;
	      }
	      rel2 {
	          relative, 1.0 1.0;
 	          offset, -1 -1;
	      }
	      color, 0 0 0 255;
	      color3, 255 255 255 32;
	      text {
	          text, "text";
		  font, "Vera";
		  size, 10;
		  min, 1 1;
		  align, 0.5 0.5;
	      }
	  }
      }
   }
}

