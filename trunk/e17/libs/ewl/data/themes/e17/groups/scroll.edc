group
{
	name: "ewl/scrollpane/background";
	data
	{
		item: "inset" 4;
	}
	parts
	{
		part
		{
			name: "bg";
			mouse_events: 0;
			description
			{
				state: "default" 0.0;
				image
				{
					normal: "e17_ibar_bg_v.png";
					border: 7 7 7 7;
				}
				fill
				{
					smooth: 0;
				}
			}
		}
		part
		{
			name: "overlay";
			mouse_events: 0;
			description
			{
				state: "default" 0.0;
				rel1
				{
					to: "bg";
				}
				rel2
				{
					to: "bg";
				}
				image
				{
					normal: "e17_ibar_over_v.png";
					border: 13 13 13 13;
					middle: 0;
				}
				fill
				{
					smooth: 0;
				}
			}
		}
	}
}

group
