group
{
	name: "ewl/seeker/vertical/button";
	min: 18 18;

	parts
	{
		part
		{
			name: "drag";
			description
			{
				state: "default" 0.0;
				min: 16 16;
				max: 16 99999;
				image
				{
					normal: "e17_sb_barv1.png";
					border: 5 5 5 5;
				}
				fill
				{
					smooth: 0;
				}
			}
			description
			{
				state: "down" 0.0;
				inherit: "default" 0.0;
				image
				{
					normal: "e17_sb_barv2.png";
				}
			}
		}
		part
		{
			name: "drag_thumb";
			mouse_events: 0;
			description
			{
				state: "default" 0.0;
				max: 8 8;
				min: 8 8;
				
				rel1
				{
					relative: 0.5 0.5;
					offset: -4 -9;
					to: "drag";
				}
				rel2
				{
					relative: 0.5 0.5;
					offset: 1 6;
					to: "drag";
				}
				image
				{
					normal: "e17_scrollbar_vdrag_thumb.png";
				}
			}
		}
	}
	programs
	{
		program
		{
			name: "drag_down";
			signal: "mouse,down,*";
			source: "EWL";
			action: STATE_SET "down" 0.0;
			target: "drag";
		}
		program
		{
			name: "drag_up";
			signal: "mouse,up,*";
			source: "EWL";
			action: STATE_SET "default" 0.0;
			target: "drag";
		}
	}
}

