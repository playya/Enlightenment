images
{
	image: "e17_button_sm_disabled.png" COMP;
	image: "e17_frame_outline.png" COMP;
	image: "ewl_notebook_tab_top_selected.png" COMP;
	image: "ewl_notebook_tab_bottom_selected.png" COMP;
	image: "ewl_notebook_tab_left_selected.png" COMP;
	image: "ewl_notebook_tab_right_selected.png" COMP;
}

group
{
	name: "ewl/notebook/pages";

	data
	{
		item: "inset" 4;
		item: "pad" 0;
	}

	parts
	{
		part
		{
			name: "outline";
			mouse_events: 0;

			description
			{
				state: "default" 0.0;
				visible: 1;
				color: 255 255 255 255;
				rel1
				{
					relative: 0.0 0.0;
					offset: 0 0;
				}
				rel2
				{
					relative: 1.0 1.0;
					offset: -1 -1;
				}
				image
				{
					normal: "e17_frame_outline.png";
					border: 7 7 7 7;
					middle: 0;
				}
				fill
				{
					smooth: 0;
				}
			}
			description
			{
				state: "hidden" 0.0;
				inherit: "default" 0.0;
				visible: 0;
				color: 255 255 255 0;
			}
		}
	}
	programs
	{
		program
		{
			name: "visible";
			signal: "tabs";
			source: "EWL";
			action: STATE_SET "default" 0.0;
			target: "outline";
		}
		program
		{
			name: "hidden";
			signal: "notabs";
			source: "EWL";
			action: STATE_SET "hidden" 0.0;
			target: "outline";
		}
	}
}

