group {
    name, "stock_ok";
    min, 16 16;
    max, 256 256;
    parts {
        part {
            name, "ok";
            type, IMAGE;
            mouse_events, 0;
            description {
                state, "default" 0.0;
                rel1 {
                    relative, 0.0 0.0;
                    offset, 0 0;
                }
                rel2 {
                    relative, 1.0 1.0;
                    offset, 0 0;
                }
                image {
                    normal, "stock-ok.png";
                }
            }
        }
    }
}

