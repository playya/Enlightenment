group {
   name: "iconbox/scrolled_view";
   min: 120 18;
   data {
      item: "inset" "6 6 6 6";
   }
   parts {
      part {
         name: "background";
         description {
            state: "default" 0.0;
            rel1 {
               relative: 0.0 0.0;
               offset: 0 0;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: -1 -1;
            }
            image {
               normal: "text_view_bg.png";
               border: 6 6 6 6;
               middle: 0;
            }
            fill {
               smooth: 0;
            }
         }
      }
      part {
         name: "middle";
         type: RECT;
         description {
            state: "default" 0.0;
            color: 255 255 255 255;
            rel1 {
               relative: 0.0 0.0;
               offset: 6 6;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: -7 -7;
            }
         }
      }
      part {
         name: "over";
         description {
            state: "default" 0.0;
            rel1 {
               relative: 0.0 0.0;
               offset: 0 0;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: -1 -1;
            }
            image {
               normal: "text_view_over.png";
               border: 13 13 13 13;
               middle: 0;
            }
            fill {
               smooth: 0;
            }
         }
      }
      FOCUS_GLOW(0.0, 0.0, -1, -1, "background", 1.0, 1.0, 0, 0, "background")
   }
   programs {
      program {
         name: "text_view_focus";
         signal: "focus";
         source: "";
         FOCUS_GLOW_ACTION
      }
      program {
         name: "text_view_unfocus";
         signal: "unfocus";
         source: "";
         UNFOCUS_GLOW_ACTION
      }
   }
}

group {
   name: "iconbox/scrolled_view/hscrollbar";
   min: 68 18;
   parts {
      part {
         name: "trough";
         description {
            state: "default" 0.0;
            rel1 {
               relative: 1.0 0.5;
               offset: -6 5;
               to_x: "left_left_arrow";
            }
            rel2 {
               relative: 0.0 0.5;
               offset: 5 6;
               to_x: "right_left_arrow";
            }
            image {
               normal: "hscrollbar_trough2.png";
	       border: 4 4 0 0;
            }
         }
      }
      part {
         name: "confine";
         type: RECT;
         description {
            state: "default" 0.0;
            visible: 0;
            rel1 {
               relative: 0.0 0.5;
               offset: 10 13;
            }
            rel2 {
               relative: 1.0 0.5;
               offset: -28 13;
            }
         }
      }
      SCROLLBAR_DRAG("drag", 20, 16, 1, 0, "scrollbar_hdrag1.png", "scrollbar_hdrag2.png", "scrollbar_hdrag_thumb.png")
      SCROLLBAR_BUTTON("left_left_arrow", 0.0, 0.5, -6, -2, "scrollbar_button_left1.png", "scrollbar_button_left2.png")
      SCROLLBAR_BUTTON("right_left_arrow", 1.0, 0.5, -27, -2, "scrollbar_button_left1.png", "scrollbar_button_left2.png")
      SCROLLBAR_BUTTON("right_right_arrow", 1.0, 0.5, -11, -2, "scrollbar_button_right1.png", "scrollbar_button_right2.png")
   }
   programs {
      SCROLLBAR_DRAG_PROG("drag")
      SCROLLBAR_BUTTON_PROG("left_left_arrow", "left")
      SCROLLBAR_BUTTON_PROG("right_left_arrow", "left")
      SCROLLBAR_BUTTON_PROG("right_right_arrow", "right")
   }
}

