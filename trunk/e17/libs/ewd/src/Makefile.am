installed_headersdir = $(prefix)/include/ewd

lib_LTLIBRARIES = libewd.la
installed_headers_DATA = \
	Ewd.h \
	ewd_config.h \
	ewd_hash.h \
	ewd_sheap.h \
	ewd_list.h \
	ewd_macros.h \
	ewd_path.h \
	ewd_plugin.h \
	ewd_strings.h \
	ewd_threads.h \
	ewd_tree.h \
	ewd_value.h

libewd_la_CFLAGS = -O2

libewd_la_SOURCES = \
	ewd_hash.c \
	ewd_sheap.c \
	ewd_list.c \
	ewd_tree.c \
	ewd_strings.c \
	ewd_value.c \
	ewd_path.c \
	ewd_plugin.c

libewd_la_LIBADD = -lm @dlopen_libs@
libewd_la_LDFLAGS = -version-info 0:0:0
