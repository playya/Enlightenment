	       rel1 {
		  relative: 0.0  0.0;
		  offset:   0    0;
	       }
	       rel2 {
		  relative: 1.0  1.0;
		  offset:   -1   -1;
	       }
	       image {
		  normal: "bg.png";
	       }
	       border: 12 12 12 12;
	    }
	 }
	 part {
	    name:   "text";
	    type:   TEXT;
            effect: SOFT_SHADOW;
	    description {
	       state:    "default" 0.0;
	       rel1 {
		  relative: 1.0  0.0;
		  offset:   0 0;
		  to:       "logo";
	       }
	       rel2 {
		  relative: 3.0 1.0;
		  offset:   -64 -1;
		  to:       "logo";
	       }
	       color_class: "fg";
	       color: 255 255 255 255;
//	       color2: 0 0 255 255;
	       color3: 0 0 0 32;
	       text {
		  text:     "This is a longer test string";
		  text_class: "plainjane";
		  font:     "Edje.Vera";
		  size:     6;
//		  fit:    0 1;
//		  min:    1 1;
		  align:   0.0 0.5;
