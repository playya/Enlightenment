collections {
	group {	name: "vertical_layout";
		parts {
			part {	name: "timeline";
				type: SWALLOW;
				description {	state: "default" 0.0;
						rel1 {
							relative: 0.0 0.0;
						}
						rel2 {
							relative: 1.0 0.0;
							to_y: "toolbar";
						}
				}
			}

			part {	name: "toolbar";
				type: SWALLOW;
				description {	state: "default" 0.0;
						align: 0.5 1.0;
						rel1 {
							relative: 0.0 1.0;
						}
						rel2 {
							relative: 1.0 1.0;
						}
						fixed: 1 1;
				}
			}
		}
	}
}
