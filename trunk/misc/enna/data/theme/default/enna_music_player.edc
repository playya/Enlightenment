#define PRG_BUTTON_PULSE(btn_name)					\
		program {									\
			name: btn_name;							\
			signal: "enna,state,"btn_name;			\
			source: "enna";							\
			action: STATE_SET btn_name 0.0;			\
			transition : "LINEAR" 0.1;				\
			target: "enna.btn."btn_name;			\
			after: btn_name"_pulse1";				\
		}											\
		program {									\
			name: btn_name"_pulse1";				\
			action: STATE_SET "default" 0.0;		\
			transition : "LINEAR" 0.3;				\
			target: "enna.btn."btn_name;			\
			after: btn_name"_pulse2";				\
		}											\
		program {									\
			name: btn_name"_pulse2";				\
			action: STATE_SET btn_name 0.0;			\
			transition : "LINEAR" 0.1;				\
			target: "enna.btn."btn_name;			\
			after: btn_name"_pulse3";				\
		}											\
		program {									\
			name: btn_name"_pulse3";				\
			action: STATE_SET "default" 0.0;		\
			transition : "LINEAR" 0.3;				\
			target: "enna.btn."btn_name;			\
			after: btn_name"_pulse4";				\
		}											\
		program {									\
			name: btn_name"_pulse4";				\
			action: STATE_SET btn_name 0.0;			\
			transition : "LINEAR" 0.1;				\
			target: "enna.btn."btn_name;			\
			after: btn_name"_pulse5";				\
		}											\
		program {									\
			name: btn_name"_pulse5";				\
			action: STATE_SET "default" 0.0;		\
			transition : "LINEAR" 0.3;				\
			target: "enna.btn."btn_name;			\
		}		

	programs {
		program {
			name: "play";
			signal: "enna,state,play";
			source: "enna";
			action: STATE_SET "play" 0.0;
			transition : "LINEAR" 0.5;
			target: "enna.btn.play";
			target: "enna.btn.pause";
			target: "enna.btn.stop";
		}

		program {
			name: "pause";
			signal: "enna,state,pause";
			source: "enna";
			action: STATE_SET "pause" 0.0;
			transition : "LINEAR" 0.5;
			target: "enna.btn.play";
			target: "enna.btn.pause";
			target: "enna.btn.stop";
		}
		program {
			name: "stop";
			signal: "enna,state,stop";
			source: "enna";
			action: STATE_SET "stop" 0.0;
			transition : "LINEAR" 0.5;
			target: "enna.btn.play";
			target: "enna.btn.pause";
			target: "enna.btn.stop";
		}
		PRG_BUTTON_PULSE("next")
		PRG_BUTTON_PULSE("prev")
	}
