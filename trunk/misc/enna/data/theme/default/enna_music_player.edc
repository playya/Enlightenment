#define BUTTON(icon_name, x, y, z)						\
		part {										\
			name: "enna.btn."icon_name;					\
			mouse_events: 0;						\
			description{							\
				state: "default" 0.0;				\
				align:    0.5 0.5;					\
				aspect:   1.0 1.0;					\
				visible: 0;							\
				aspect_preference: VERTICAL;		\
				color: 255 255 255 0;				\
				image {normal: "icon_"icon_name".png";}	\
			}										\
			description{							\
				state: "state1" 0.0;				\
				inherit: "default" 0.0;				\
				color: 255 255 255 200*x;			\
				visible: x;    						\
			}										\
			description{							\
				state: "state2" 0.0;				\
				inherit: "default" 0.0;				\
				color: 255 255 255 200*y; 			\
				visible: y;							\
			}										\
			description{							\
				state: "state3" 0.0;				\
				inherit: "default" 0.0;				\
				color: 255 255 255 200*z;			\
				visible: z;							\
			}										\
		}	

