                        name, "swallow_backdrop";
                        type, IMAGE;
                        mouse_events, 0;
                        description {
                                state,  "default" 0.0;

                                rel1 {
                                        relative, 0.0 0.0;
                                        offset, 4 92;
                                }

                                rel2 {
                                        relative, 1.0 1.0;
                                        offset, -5 -5;
                                }

                                image {
                                        normal, "lcd_area.png";
                                        border, 5 5 5 5;
                                }

                                fill {
                                        smooth, 0;
                                }
                        }
                }

                part {
                        name, "swallow";
                        type, RECT;
                        mouse_events, 1;

                        description {
                                state, "default" 0.0;

                                rel1 {
                                        relative, 0.0 0.0;
                                        offset, 8 5;
                                        to, "swallow_backdrop";
                                }

                                rel2 {
                                        relative, 1.0 1.0;
                                        offset, -6 -6;
                                        to, "swallow_backdrop";
                                }
                                color, 0 0 0 0;
                        }
                }

