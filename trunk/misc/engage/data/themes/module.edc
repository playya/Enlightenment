	 }
	 description {
	    state:    "right" 0.0;
	    fixed: 1 1;
	    rel1 {
	       relative: 0.0  0.0;
	       offset:   0    0;
	       to: "background";
	    }
	    rel2 {
	       relative: 1.0  0.0;
	       offset:   -3   2;
	       to_x: "background";
	    }
            align: 0.5 0.0;
