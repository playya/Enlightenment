#!/bin/bash
[ -x ./feh ] && {
FEH="./feh"
} || {
FEH="feh"
}
echo ""
echo "Don't worry. This is designed to fail, in order to test error handling"
echo "The app should output meaningful error information to explain failure"
echo ""
$FEH $1 -V testimages/1.jpg testimages/2.jpg testimages/3.jpg testimages/4.jpg
echo ""
