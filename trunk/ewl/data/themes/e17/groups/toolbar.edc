images
{
	image: "e17_button_sm_disabled.png" COMP;
	image: "e17_button_sm_disabled2.png" COMP;
}

group
{
	name: "ewl/toolbar/icon";

	data
	{
		item: "pad" 0;
		item: "inset" 8;
	}

	parts
	{
		part
		{
			name: "button_image";
			type: IMAGE;
			mouse_events: 1;
			description
			{
				state: "default" 0.0;
				visible: 0;
				rel1
				{
					relative: 0.0 0.0;
					offset: 0 0;
				}
				rel2
				{
					relative: 1.0 1.0;
					offset: -1 -1;
				}
				image
				{
					normal: "e17_button_sm_disabled2.png";
					border: 8 8 8 8;
				}
			}
			description
			{
				state: "highlighted" 0.0;
				inherit: "default" 0.0;
				visible: 1;
			}
			description
			{
				state: "pressed" 0.0;
				inherit: "default" 0.0;
				visible: 1;
				image
				{
					normal: "e17_button_sm_disabled.png";
					border: 8 8 8 8;
				}
			}
		}
	}
	programs
	{
		program
		{
			name: "highlight";
			signal: "mouse,in";
			source: "EWL";
			action: STATE_SET "highlighted" 0.0;
			target: "button_image";
			transition: DECELERATE 1.0;
		}
		program
		{
			name: "un-highlight";
			signal: "mouse,out";
			source: "EWL";
			action: STATE_SET "default" 0.0;
			target: "button_image";
			transition: LINEAR 0.01;
		}
		program
		{
			name: "clicked";
			signal: "mouse,down,*";
			source: "EWL";
			action: STATE_SET "pressed" 0.0;
			target: "button_image";
			transition: LINEAR 0.0;
		}
		program
		{
			name: "release";
			signal: "mouse,up,*";
			source: "EWL";
			action: STATE_SET "highlighted" 0.0;
			target: "button_image";
			transition: LINEAR 0.01;
		}
	}
}


