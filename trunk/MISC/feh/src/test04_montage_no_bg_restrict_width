#!/bin/bash
[ -x ./feh ] && {
FEH="./feh"
} || {
FEH="feh"
}
echo ""
echo "This test should create a montage limited to 300 pixels in width."
echo "The thumbnails should be 50x50 and retain aspect ratio"
echo ""
$FEH $1 -mVo _test04.jpg --thumb-width 50 --thumb-height 50 --limit-width 300 testimages
echo ""
