                description
                {
                    state: "default" 0.0;
                    aspect: 1.0 1.0;
                    aspect_preference: BOTH;
                    rel1.offset: 2 2;
                    rel2.offset: -3 -3;
                    image.normal: "module_icon.png";
                    color: 255 255 255 255;
                }
                description
                {
                    state: "hide" 0.0;
                    inherit: "default" 0.0;
                    visible: 0;
                    color: 255 255 255 0;
                }
            }
            part
            {
                name: "icon_2";
                mouse_events: 0;
