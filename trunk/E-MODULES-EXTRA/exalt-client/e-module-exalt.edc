                }
                description
                {
                    state: "hide" 0.0;
                    inherit: "default" 0.0;
                    visible: 1;
                    color: 255 255 255 255;
                }
            }
            part
            {
                name: "glow";
                mouse_events: 0;
                type: IMAGE;
                description
                {
                    state: "default" 0.0;
                    aspect: 1.0 1.0;
                    aspect_preference: BOTH;
                    rel1.offset: 2 2;
                    rel2.offset: -3 -3;
                    visible: 0;
                    color: 255 255 255 0;
                    image.normal: "module_icon_glow.png";
                }
                description
                {
                    state: "glow" 0.0;
                    inherit: "default" 0.0;
                    visible: 1;
                    color: 255 255 255 255;
                }
            }
            part
            {
                name: "event";
                mouse_events: 1;
                type: RECT;
                description
                {
                    state: "default" 0.0;
                    color: 255 255 255 0;
                }
            }
        }
        programs
        {
            program
            {
                name: "mouse_in";
                signal: "mouse,in";
                source: "event";
                action: STATE_SET "glow" 0.0;
                transition: ACCELERATE 0.3;
                target: "glow";
            }
            program
            {
                name: "mouse_out";
                signal: "mouse,out";
                source: "event";
                action: STATE_SET "default" 0.0;
                transition: DECELERATE 0.5;
                target: "glow";
            }

            program
            {
                name: "apply_start";
                signal: "apply,start";
                source: "exalt";
                action: STATE_SET "hide" 0.0;
                target: "icon";
                target: "icon_2";
                transition: ACCELERATE 0.7;
                after: "apply_start_2";
            }

            program
            {
                name: "apply_start_2";
                action: STATE_SET "default" 0.0;
                target: "icon";
                target: "icon_2";
                transition: ACCELERATE 0.7;
                after: "apply_start";
            }

            program
            {
                name: "apply_stop";
                signal: "apply,stop";
                source: "exalt";
                action: ACTION_STOP;
                target: "apply_start";
                after: "apply_stop_2";
            }
            program
            {
                name: "apply_stop_2";
                signal: "apply,stop_2";
                source: "exalt";
                action: STATE_SET "default" 0.0;
                target: "icon";
                target: "icon_2";
            }
        }
    }

