            name: "desktop,show";
            signal: "elfe,desktop,show";
            source: "elfe";
            action: STATE_SET "show" 0.0;
            target: "elfe.swallow.desktop";
            transition: "DECELERATE" 0.5;
         }
         program {
            name: "desktop,hide";
            signal: "elfe,desktop,hide";
            source: "elfe";
            action: STATE_SET "default" 0.0;
            target: "elfe.swallow.desktop";
            transition: "ACCELERATE" 0.5;
         }

      }
   }
