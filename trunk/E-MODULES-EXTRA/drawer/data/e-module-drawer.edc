/*
 * vim:ts=8:sw=3:sts=8:noexpandtab
 */
images {
   image: "module_icon.png" COMP;
   image: "env_bg_top.png" COMP;
   image: "env_fg_top.png" COMP;
   image: "env_bg_right.png" COMP;
   image: "env_fg_right.png" COMP;
   image: "env_bg_bottom.png" COMP;
   image: "env_fg_bottom.png" COMP;
   image: "env_bg_left.png" COMP;
   image: "env_fg_left.png" COMP;
   image: "logo_white_128.png" COMP;
   image: "popup_bg.png" COMP;
   image: "popup_overlay.png" COMP;
   image: "bt_glow.png" COMP;
   image: "gadman_border.png" COMP;
   image: "h_line.png" COMP;
   image: "v_line.png" COMP;
   image: "bt_sm_base1.png" COMP;
   image: "bt_sm_shine.png" COMP;
   image: "bt_sm_hilight.png" COMP;
}

collections {
   group {
      name: "icon";
      max: 24 24;
      parts {
	 part {
	    name: "image";
	    mouse_events: 0;
	    type: IMAGE;
	    description {
	       state: "default" 0.0;
	       image.normal: "module_icon.png";
	    }
	 }
      }
   }
   group {
      name: "modules/drawer/main";
      max: 128 128;
      script {
	 public popup_visible;
	 public orient; /* 0 - default, 1 - left, 2 - top, 3 - right */
      }
      parts {

#undef ORIENT_TARGETS
#define ORIENT_TARGETS \
target: "bg"; \
target: "fg";

	 part {
	    name: "bg";
	    mouse_events: 0;
	    type: IMAGE;
	    description {
	       state: "default" 0.0;
	       aspect: 1.0 1.0;
	       aspect_preference: BOTH;
	       rel1.offset: 1 1;
	       rel2.offset: -2 -2;
	       image {
		  normal: "env_bg_bottom.png";
	       }
	    }
	    description {
	       state: "left" 0.0;
	       inherit: "default" 0.0;
	       image.normal: "env_bg_left.png";
	    }
	    description {
	       state: "top" 0.0;
	       inherit: "default" 0.0;
	       image.normal: "env_bg_top.png";
	    }
	    description {
	       state: "right" 0.0;
	       inherit: "default" 0.0;
	       image.normal: "env_bg_right.png";
	    }
	 }
	 part {
	    name: "content_clipper";
	    type: RECT;
	    description {
	       state: "default" 0.0;
	       aspect: 1 1;
	       aspect_preference: BOTH;
	       rel1.to: "bg";
	       rel2.to: "bg";
	    }
	    description {
	       state: "over_bottom" 0.0;
	       inherit: "default" 0.0;
	       rel1 {
		  relative: 0 -0.15;
		  to: "bg";
	       }
	       rel2 {
		  relative: 1 0.85;
		  to: "bg";
	       }
	    }
	    description {
	       state: "over_left" 0.0;
	       inherit: "default" 0.0;
	       rel1 {
		  relative: 0.15 0;
		  to: "bg";
	       }
	       rel2 {
		  relative: 1.15 1;
		  to: "bg";
	       }
	    }
	    description {
	       state: "over_top" 0.0;
	       inherit: "default" 0.0;
	       rel1 {
		  relative: 0 0.15;
		  to: "bg";
	       }
	       rel2 {
		  relative: 1 1.15;
		  to: "bg";
	       }
	    }
	    description {
	       state: "over_right" 0.0;
	       inherit: "default" 0.0;
	       rel1 {
		  relative: -0.15 0;
		  to: "bg";
	       }
	       rel2 {
		  relative: 0.85 1;
		  to: "bg";
	       }
	    }
	    description {
	       state: "popup_show_bottom" 0.0;
	       inherit: "over_bottom" 0.0;
	       rel1 {
		  relative: 0 -1.5;
		  to: "bg";
	       }
	       rel2 {
		  relative: 1 -0.5;
		  to: "bg";
	       }
	       color: 255 255 255 0;
	    }
	    description {
	       state: "popup_show_left" 0.0;
	       inherit: "over_left" 0.0;
	       rel1 {
		  relative: 1.5 0;
		  to: "bg";
	       }
	       rel2 {
		  relative: 2.5 1;
		  to: "bg";
	       }
	       color: 255 255 255 0;
	    }
	    description {
	       state: "popup_show_top" 0.0;
	       inherit: "over_top" 0.0;
	       rel1 {
		  relative: 0 1.5;
		  to: "bg";
	       }
	       rel2 {
		  relative: 1 2.5;
		  to: "bg";
	       }
	       color: 255 255 255 0;
	    }
	    description {
	       state: "popup_show_right" 0.0;
	       inherit: "over_right" 0.0;
	       rel1 {
		  relative: -1.5 0;
		  to: "bg";
	       }
	       rel2 {
		  relative: -0.5 1;
		  to: "bg";
	       }
	       color: 255 255 255 0;
	    }
	 }
	 part {
	    name: "e.swallow.content";
	    clip_to: "content_clipper";
	    type: SWALLOW;
	    description {
	       state: "default" 0.0;
	       rel1.to: "content_clipper";
	       rel2.to: "content_clipper";
	    }
	 }
	 part {
	    name: "fg";
	    mouse_events: 0;
	    type: IMAGE;
	    description {
	       state: "default" 0.0;
	       aspect: 1.0 1.0;
	       aspect_preference: BOTH;
	       rel1.to: "bg";
	       rel2.to: "bg";
	       image.normal: "env_fg_bottom.png";
	    }
	    description {
	       state: "left" 0.0;
	       inherit: "default" 0.0;
	       image.normal: "env_fg_left.png";
	    }
	    description {
	       state: "top" 0.0;
	       inherit: "default" 0.0;
	       image.normal: "env_fg_top.png";
	    }
	    description {
	       state: "right" 0.0;
	       inherit: "default" 0.0;
	       image.normal: "env_fg_right.png";
	    }
	 }
	 part {
	    name: "event";
	    mouse_events: 1;
	    type: RECT;
	    description {
	       state: "default" 0.0;
	       color: 255 255 255 0;
	    }
	 }
      }
      programs {
	 program {
	    name: "init";
	    signal: "load";
	    source: "";
	    script {
	       set_int(popup_visible, 0);
	    }
	 }
	 program {
	    name: "mouse_in";
	    signal: "mouse,in";
	    source: "event";
	    script {
	       if (get_int(popup_visible) == 0) {
		  if (get_int(orient) == 0)
		     run_program(PROGRAM:"mouse_in2_bottom");
		  else if (get_int(orient) == 1)
		     run_program(PROGRAM:"mouse_in2_left");
		  else if (get_int(orient) == 2)
		     run_program(PROGRAM:"mouse_in2_top");
		  else if (get_int(orient) == 3)
		     run_program(PROGRAM:"mouse_in2_right");
	       }
	    }
	 }
	 program {
	    name: "mouse_in2_bottom";
	    action: STATE_SET "over_bottom" 0.0;
	    transition: LINEAR 0.1;
	    target: "content_clipper";
	 }
	 program {
	    name: "mouse_in2_left";
	    action: STATE_SET "over_left" 0.0;
	    transition: LINEAR 0.1;
	    target: "content_clipper";
	 }
	 program {
	    name: "mouse_in2_top";
	    action: STATE_SET "over_top" 0.0;
	    transition: LINEAR 0.1;
	    target: "content_clipper";
	 }
	 program {
	    name: "mouse_in2_right";
	    action: STATE_SET "over_right" 0.0;
	    transition: LINEAR 0.1;
	    target: "content_clipper";
	 }

	 program {
	    name: "mouse_out";
	    signal: "mouse,out";
	    source: "event";
	    script {
	       if (get_int(popup_visible) == 0) {
		  run_program(PROGRAM:"mouse_out2");
	       }
	    }
	 }
	 program {
	    name: "mouse_out2";
	    action: STATE_SET "default" 0.0;
	    transition: LINEAR 0.2;
	    target: "content_clipper";
	 }
	 program {
	    name: "popup_show";
	    signal: "e,action,popup,show,*";
	    source: "drawer";
	    script {
	       set_int(popup_visible, 1);
	       if (get_int(orient) == 0)
		  run_program(PROGRAM:"popup_show2_bottom");
	       else if (get_int(orient) == 1)
		  run_program(PROGRAM:"popup_show2_left");
	       else if (get_int(orient) == 2)
		  run_program(PROGRAM:"popup_show2_top");
	       else if (get_int(orient) == 3)
		  run_program(PROGRAM:"popup_show2_right");
	    }
	 }
	 program {
	    name: "popup_show2_bottom";
	    action: STATE_SET "popup_show_bottom" 0.0;
	    transition: ACCELERATE 0.3;
	    target: "content_clipper";
	 }
	 program {
	    name: "popup_show2_left";
	    action: STATE_SET "popup_show_left" 0.0;
	    transition: ACCELERATE 0.3;
	    target: "content_clipper";
	 }
	 program {
	    name: "popup_show2_top";
	    action: STATE_SET "popup_show_top" 0.0;
	    transition: ACCELERATE 0.3;
	    target: "content_clipper";
	 }
	 program {
	    name: "popup_show2_right";
	    action: STATE_SET "popup_show_right" 0.0;
	    transition: ACCELERATE 0.3;
	    target: "content_clipper";
	 }
	 program {
	    name: "popup_hide";
	    signal: "e,action,popup,hide,*";
	    source: "drawer";
	    after: "popup_hide2";
	 }
	 program {
	    name: "popup_hide2";
	    action: STATE_SET "default" 0.0;
	    transition: DECELERATE 0.5;
	    target: "content_clipper";
	    after: "popup_hide3";
	 }
	 program {
	    name: "popup_hide3";
	    script {
	       set_int(popup_visible, 0);
	    }
	 }

	 program {
	    name: "orient_bottom";
	    signal: "e,state,orient,bottom";
	    source: "drawer";
	    action: STATE_SET "default" 0.0;
	    ORIENT_TARGETS
	    after: "orient_bottom_scr";
	 }
	 program {
	    name: "orient_bottom_scr";
	    script {
	       set_int(orient, 0);
	    }
	 }
	 program {
	    name: "orient_left";
	    signal: "e,state,orient,left";
	    source: "drawer";
	    action: STATE_SET "left" 0.0;
	    ORIENT_TARGETS
	    after: "orient_left_scr";
	 }
	 program {
	    name: "orient_left_scr";
	    script {
	       set_int(orient, 1);
	    }
	 }
	 program {
	    name: "orient_top";
	    signal: "e,state,orient,top";
	    source: "drawer";
	    action: STATE_SET "top" 0.0;
	    ORIENT_TARGETS
	    after: "orient_top_scr";
	 }
	 program {
	    name: "orient_top_scr";
	    script {
	       set_int(orient, 2);
	    }
	 }
	 program {
	    name: "orient_right";
	    signal: "e,state,orient,right";
	    source: "drawer";
	    action: STATE_SET "right" 0.0;
	    ORIENT_TARGETS
	    after: "orient_right_scr";
	 }
	 program {
	    name: "orient_right_scr";
	    script {
	       set_int(orient, 3);
	    }
	 }
      }
   }
   group {
      name: "modules/drawer/main/content";
      parts {
	 part {
	    name: "image";
	    mouse_events: 0;
	    type: IMAGE;
	    description {
	       state: "default" 0.0;
	       visible: 1;
	       image.normal: "logo_white_128.png";
	    }
	 }
      }
   }
   group {
      name: "modules/drawer/main_float";
      parts {
	 part {
	    name:	"e.text.description";
	    type:	TEXT;
	    effect:	GLOW;
	    scale: 1;
	    description {
	       state: "default" 0.0;
	       rel1.offset: 10 1;
	       rel2 {
		  relative: 1 0;
		  offset: -1 10;
	       }
	       color: 255 255 255 255;
	       color3: 0 0 0 90;
	       text {
		  font: "Sans:style=Bold";
		  align: 0 0.5;
		  min: 0 1;
		  elipsis: 0;
		  size: 10;
	       }
	    }
	 }
	 part {
	    name: "border";
	    description {
	       state: "default" 0.0;
	       rel1 {
		  relative: 0 1;
		  offset: 0 -7;
		  to_y: "e.text.description";
	       }
	       image {
		  normal: "gadman_border.png";
		  border: 8 8 8 8;
		  middle: 0;
	       }
	       fill.smooth: 0;
	    }
	 }
	 part {
	    name: "e.swallow.content";
	    type: SWALLOW;
	    description {
	       state: "default" 0.0;
	       rel1 {
		  relative: 0 0;
		  offset: 8 8;
		  to: "border";
	       }
	       rel2.offset: -9 -9;
	    }
	 }
      }
   }
   group {
      name: "modules/drawer/container";
      data {
	 item: "shaped" "1";
      }
      parts {
#undef TARGETS
#define TARGETS \
target: "bg";

	 part {
	    name:          "bg";
	    mouse_events:  0;
	    description {
	       state:    "default" 0.0;
	       rel1.relative: 0 0;
	       rel2.relative: 1 1;
	       image {
		  normal: "popup_bg.png";
		  border: 10 10 15 10;
	       }
	    }
	    description {
	       state: "hidden_bottom" 0.0;
	       inherit: "default" 0.0;
	       rel1.relative: 0 1;
	       rel2.relative: 1 1;
	    }
	    description {
	       state: "hidden_left" 0.0;
	       inherit: "default" 0.0;
	       rel1.relative: 0 0;
	       rel2.relative: 0 1;
	    }
	    description {
	       state: "hidden_top" 0.0;
	       inherit: "default" 0.0;
	       rel1.relative: 0 0;
	       rel2.relative: 1 0;
	    }
	    description {
	       state: "hidden_right" 0.0;
	       inherit: "default" 0.0;
	       rel1.relative: 1 0;
	       rel2.relative: 1 1;
	    }
	 }
	 part {
	    name:	"e.text.description";
	    type:	TEXT;
	    effect:	GLOW;
	    mouse_events:  0;
	    scale: 1;
	    description {
	       state: "default" 0.0;
	       fixed: 0 1;
	       visible: 1;
	       rel1 {
		  offset: 20 1;
		  to: "bg";
	       }
	       rel2 {
		  relative: 1 0;
		  offset: -1 15;
		  to: "bg";
	       }
	       color: 255 255 255 255;
	       color3: 0 0 0 90;
	       text {
		  font: "Sans:style=Bold";
		  align: 0 0.5;
		  min: 0 1;
		  elipsis: 0;
		  size: 10;
	       }
	    }
	 }
	 part {
	    name: "clipper";
	    mouse_events:  0;
	    type: RECT;
	    description {
	       state: "default" 0.0;
	       visible: 1;
	       color: 255 255 255 255;
	       rel1 {
		  relative: 0 0;
		  offset: 9 14;
		  to: "bg";
	       }
	       rel2 {
		  offset: -10 -10;
		  to: "bg";
	       }
	    }
	 }
	 part {
	    name: "e.swallow.content";
	    type: SWALLOW;
	    clip_to: "clipper";
	    description {
	       state: "default" 0.0;
	       align: 0.5 0.5;
	       color: 255 255 255 255;
	       rel1.to: "clipper";
	       rel2.to: "clipper";
	    }
	 }
	 part {
	    name: "overlay";
	    repeat_events: 1;
	    description {
	       state: "default" 0.0;
	       visible: 1;
	       color: 255 255 255 255;
	       rel1 {
		  offset: 1 1;
		  to: "bg";
	       }
	       rel2 {
		  offset: -2 -2;
		  to: "bg";
	       }
	       image {
		  normal: "popup_overlay.png";
		  border: 10 10 25 10;
	       }
	       fill.smooth: 1;
	    }
	 }
      }
      programs {
	 program {
	    name: "popup_show_bottom";
	    signal: "e,action,popup,show,bottom";
	    source: "drawer";
	    action: STATE_SET "hidden_bottom" 0.0;
	    TARGETS
	    after: "popup_show2";
	 }
	 program {
	    name: "popup_show2";
	    action: STATE_SET "default" 0.0;
	    transition: DECELERATE 0.5;
	    TARGETS
	 }
	 program {
	    name: "popup_hide_bottom";
	    signal: "e,action,popup,hide,bottom";
	    source: "drawer";
	    action: STATE_SET "default" 0.0;
	    TARGETS
	    after: "popup_hide2_bottom";
	 }
	 program {
	    name: "popup_hide2_bottom";
	    action: STATE_SET "hidden_bottom" 0.0;
	    transition: ACCELERATE 0.5;
	    TARGETS
	    after: "popup_hide3";
	 }
	 program {
	    name: "popup_show_left";
	    signal: "e,action,popup,show,left";
	    source: "drawer";
	    action: STATE_SET "hidden_left" 0.0;
	    TARGETS
	    after: "popup_show2";
	 }
	 program {
	    name: "popup_hide_left";
	    signal: "e,action,popup,hide,left";
	    source: "drawer";
	    action: STATE_SET "default" 0.0;
	    TARGETS
	    after: "popup_hide2_left";
	 }
	 program {
	    name: "popup_hide2_left";
	    action: STATE_SET "hidden_left" 0.0;
	    transition: ACCELERATE 0.5;
	    TARGETS
	    after: "popup_hide3";
	 }
	 program {
	    name: "popup_show_top";
	    signal: "e,action,popup,show,top";
	    source: "drawer";
	    action: STATE_SET "hidden_top" 0.0;
	    TARGETS
	    after: "popup_show2";
	 }
	 program {
	    name: "popup_hide_top";
	    signal: "e,action,popup,hide,top";
	    source: "drawer";
	    action: STATE_SET "default" 0.0;
	    TARGETS
	    after: "popup_hide2_top";
	 }
	 program {
	    name: "popup_hide2_top";
	    action: STATE_SET "hidden_top" 0.0;
	    transition: ACCELERATE 0.5;
	    TARGETS
	    after: "popup_hide3";
	 }
	 program {
	    name: "popup_show_right";
	    signal: "e,action,popup,show,right";
	    source: "drawer";
	    action: STATE_SET "hidden_right" 0.0;
	    TARGETS
	    after: "popup_show2";
	 }
	 program {
	    name: "popup_hide_right";
	    signal: "e,action,popup,hide,right";
	    source: "drawer";
	    action: STATE_SET "default" 0.0;
	    TARGETS
	    after: "popup_hide2_right";
	 }
	 program {
	    name: "popup_hide2_right";
	    action: STATE_SET "hidden_right" 0.0;
	    transition: ACCELERATE 0.5;
	    TARGETS
	    after: "popup_hide3";
	 }

	 program {
	    name: "popup_hide3";
	    action: SIGNAL_EMIT "e,action,popup,hidden" "drawer";
	    after: "popup_hide4";
	 }
	 program {
	    name: "popup_hide4";
	    action: STATE_SET "default" 0.0;
	    TARGETS
	 }
      }
   }
   group {
      name: "modules/drawer/list/horizontal";
      parts {
	 part {
	    name: "swallow_clipper";
	    type: RECT;
	    description {
	       state: "default" 0.0;
	       color: 255 255 255 255;
	       visible: 1;
	    }
	    description {
	       state: "hidden" 0.0;
	       inherit: "default" 0.0;
	       color: 0 0 0 0;
	       visible: 0;
	    }
	 }
	 part {
	    name: "e.swallow.content";
	    type: SWALLOW;
	    clip_to: "swallow_clipper";
	    description {
	       state: "default" 0.0;
	       rel2.offset: -1 -25;
	    }
	 }
	 part {
	    name: "e.text.label";
	    type: TEXT;
	    effect: OUTLINE_SOFT_SHADOW;
	    mouse_events: 0;
	    description {
	       state: "default" 0.0;
	       color: 45 148 212 255;
	       color2: 37 48 48 64;
	       color3: 0 0 0 64;
	       fixed: 0 1;
	       visible: 1;
	       rel1 {
		  relative: 0 1;
		  offset: 1 -15;
		  to_y: "e.swallow.content";
	       }
	       rel2 {
		  relative: 1 1;
		  offset: -1 5;
		  to_y: "e.swallow.content";
	       }
	       text {
		  text: "";
		  align: 0.5 0.5;
		  font: "Sans:style=Bold";
		  size: 16;
		  elipsis: 0;
		  text_class: "module_normal";
	       }
	    }
	    description {
	       state: "activate" 0.0;
	       inherit: "default" 0.0;
	       color: 255 15 0 255;
