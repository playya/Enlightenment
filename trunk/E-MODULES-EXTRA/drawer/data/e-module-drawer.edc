	    description {
	       state: "hidden" 0.0;
	       inherit: "default" 0.0;
	       color: 0 0 0 0;
	       color2: 0 0 0 0;
	       color3: 0 0 0 0;
	       visible: 0;
	    }
	 }
	 part {
	    name: "e.text.description";
	    type: TEXT;
	    effect: OUTLINE_SOFT_SHADOW;
	    mouse_events: 0;
	    description {
	       state: "default" 0.0;
	       color: 255 255 255 255;
	       color2: 37 48 48 64;
	       color3: 37 48 48 64;
	       fixed: 0 1;
	       visible: 1;
	       rel1 {
		  relative: 0 1;
		  offset: 1 1;
		  to_y: "e.text.label";
	       }
	       rel2 {
		  relative: 1 1;
		  offset: -1 -3;
	       }
	       text {
		  text: "";
		  align: 0.5 0.5;
		  font: "Sans";
		  size: 12;
		  elipsis: 0;
		  text_class: "module_small";
	       }
	    }
	    description {
	       state: "hidden" 0.0;
	       inherit: "default" 0.0;
	       color: 0 0 0 0;
	       color2: 0 0 0 0;
	       color3: 0 0 0 0;
	       visible: 0;
	    }
	 }
      }
      programs {
	 program {
	    name: "activate_pre";
	    signal: "e,action,activate";
	    source: "drawer";
	    action: STATE_SET "default" 0.0;
