images
{
  image: "eth_dev.png" COMP;
}

fonts
{
  font: "VeraMono.ttf" "VeraMono";
}

collections
{
group
    {
      name: "modules/net/main";
      script {
		public message(Msg_Type:type, id, ...) {
		      if (type == MSG_STRING_SET) {
		      		new in_str[64];
		      		new out_str[64];		      		
				getsarg(2, in_str, sizeof(in_str));
				getsarg(3, out_str, sizeof(out_str));
				set_text(PART:"in-text", in_str);				
				set_text(PART:"out-text", out_str);								
		      }
	       }      
      }
      
      parts{
      part {
      	   name: "base";
	   type: RECT;
	   mouse_events: 0;
	   description {
	   	       state: "default" 0.0;
		       color: 255 255 255 255;
	   }
      }
      part {
	 name:          "net";
	 clip_to: "base";
	 mouse_events:  0;
	 description {
	    state:    "default" 0.0;
	    color: 255 255 255 200;
	    aspect:   1.0 1.0;
	    rel1 { 
	    	 offset: 0 6; 
	    }
	    rel2 { 
	    	 offset: -1 -6; 
	    }
	    image { 
	    	  normal: "eth_dev.png"; 
	    }
	    fill { 
	    	 smooth: 0;  
	    }
	 }
      }

      part 
	  {
	    name:           "out-text";
	    clip_to: 	    "base";
	    type:           TEXT;
	    effect:         SOFT_SHADOW;
	    mouse_events: 0;
	    description {
	      state: "default" 0.0;
	      align: 1.0 0.0;
	      rel1 { 
	      	   relative: 1.0 0.0; 
		   to_x: "net"; 
		   offset: -2 1; 
	      }
	      rel2 { 
	      	   relative: 1.0 0.0; 
	      }
	      color: 51 181 69 255;
	      color3: 0 0 0 100;
	      text {
		   text: ""; 
		   font: "VeraMono";
		   size:  8;     
		   min:     1 1;
		   align: 1.0 0.0;
	      }
	    }
	  }

	part 
	  {
	    name:           "in-text";
	    clip_to: 	    "base";
	    type:           TEXT;
	    effect:         SOFT_SHADOW;
	    mouse_events: 0;
	    description {
	      state: "default" 0.0;
	      align: 1.0 0.0;
	      rel1 { 
	      	   relative: 1.0 1.0; 
		   to_x: "net"; 
		   offset: -2 -12;  
	      }
	      rel2 { 
	      	   relative: 1.0 1.0; 
	      }
	      color: 213 91 91 255;
	      color3: 0 0 0 100;
	      text {
		   text: ""; 
		   font: "VeraMono";
		   size:  8;     
		   min:     1 1;
		   align: 1.0 0.0;
	      }
	    }
	  }
      } 
    }//close group net
}

