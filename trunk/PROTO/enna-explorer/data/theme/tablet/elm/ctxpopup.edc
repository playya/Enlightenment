group { name: "elm/ctxpopup/arrow/default";
   images {
      image: "popup_left.png" COMP;
      image: "popup_right.png" COMP;
      image: "popup_up.png" COMP;
      image: "popup_down.png" COMP;
   }
   parts {
      part { name: "ctxpopup_arrow";
         type: IMAGE;
         scale: 1;
         description {
            state: "default" 0.0;
            min: 12 12;
            fixed: 1 1;
            visible: 0;
            align: 0.5 0.5;
         }
         description {
            state: "left" 0.0;
            min: 7 12;
            fixed: 1 1;
            align: 0.0 0.5;
            //rel1 { offset: 2 0; }
            //rel2 { offset: 2 0; }
            image { normal: "popup_left.png"; }
         }
         description { state: "right" 0.0;
            min: 7 12;
            fixed: 1 1;
            align: 1.0 0.5;
            //rel1 { offset: -5 0; }
            //rel2 { offset: -6 -1; }
            image { normal: "popup_right.png"; }
         }
         description { state: "top" 0.0;
            min: 12 7;
            fixed: 1 1;
            align: 0.5 0.0;
            //rel1 { offset: 0 5; }
            //rel2 { offset: -1 4; }
            image { normal: "popup_up.png"; }
         }
         description { state: "bottom" 0.0;
            min: 12 7;
            fixed: 1 1;
            align: 0.5 1.0;
            //rel1 { offset: 0 -5; }
            //rel2 { offset: -1 -6; }
            image { normal: "popup_down.png"; }
         }
      }
   }
   programs {
      program { name: "enable_left_arrow";
         signal: "elm,state,left";
         source: "elm";
         action: STATE_SET "left" 0.0;
         target: "ctxpopup_arrow";
      }
      program { name: "enable_right_arrow";
         signal: "elm,state,right";
         source: "elm";
         action: STATE_SET "right" 0.0;
         target: "ctxpopup_arrow";
      }
      program { name: "enable_top_arrow";
         signal: "elm,state,top";
         source: "elm";
         action: STATE_SET "top" 0.0;
         target: "ctxpopup_arrow";
      }
      program { name: "enable_bottom_arrow";
         signal: "elm,state,bottom";
         source: "elm";
         action: STATE_SET "bottom" 0.0;
         target: "ctxpopup_arrow";
      }
   }
}

///////////////////////////////////////////////////////////////////////////////
group {
   name: "elm/ctxpopup/icon_text_style_item/default";
   alias: "elm/ctxpopup/text_style_item/default";
   alias: "elm/ctxpopup/icon_style_item/default";
   images {
      image: "list_selected.png" COMP;
   }
   parts {
      part { name: "event";
         mouse_events: 1;
         description { state: "default" 0.0;
         }
      }
      part { name: "bg";
         mouse_events: 0;
         description { state: "default" 0.0;
            //rel1.offset: 2 2;
            //rel2.offset: -3 -3;
            image { normal:"list_selected.png";
               border: 4 4 4 4;
            }
            color: 255 255 255 0;
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
         }
      }
      part { name: "elm.swallow.icon";
         type: SWALLOW;
         clip_to: "disclip";
         scale: 1;
         description { state: "default" 0.0;
            min: 24 24;
            max: 24 24;
            align: 0 0.5;
            aspect: 1.0 1.0;
            aspect_preference: BOTH;
            rel1 { offset: 4 3; }
            rel2 { offset: 0 -4; }
         }
      }
      part { name: "elm.text";
         type: TEXT;
         mouse_events: 0;
         clip_to: "disclip";
         scale: 1;
         description { state: "default" 0.0;
            //min: 1 40;
            fixed: 0 1;
            align: 0.5 0.5;
            rel1 {
               relative: 1.0 0.0;
               to_x: "elm.swallow.icon";
               offset: 4 3;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: -5 -4;
            }
            color: 220 220 220 255;
            text {
               font: "Sans";
               size: 10;
               align: 0.0 0.5;
               min: 1 1;
            }
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
         }
      }
      part { name: "over1";
         mouse_events: 1;
         repeat_events: 1;
         ignore_flags: ON_HOLD;
         description { state: "default" 0.0;
            color: 255 255 255 0;
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
         }
      }
      part { name: "over2";
         mouse_events: 1;
         repeat_events: 1;
         description { state: "default" 0.0;
            color: 255 255 255 0;
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
         }
      }
      part { name: "disclip";
         type: RECT;
         description { state: "default" 0.0;
            color: 255 255 255 255;
         }
         description { state: "enabled" 0.0;
            color: 127 127 127 127;
         }
      }
      part { name: "blocker";
         description { state: "default" 0.0;
            visible: 0;
         }
         description { state: "enabled" 0.0;
            visible: 1;
            color: 0 0 0 0;
         }
      }
   }
   programs {
      program {
         name: "item_unclick";
         signal: "mouse,up,1";
         source: "over1";
         action: SIGNAL_EMIT "elm,action,click" "";
      }
      program { name: "disable";
         signal: "elm,state,disabled";
         source: "elm";
         action: STATE_SET "enabled" 0.0;
         target: "disclip";
         target: "blocker";
      }
      program { name: "enable";
         signal: "elm,state,enabled";
         source: "elm";
         action: STATE_SET "default" 0.0;
         target: "disclip";
         target: "blocker";
      }
      program { name: "item_click2";
         signal: "mouse,down,1";
         source: "over2";
         script {
            set_state(PART:"elm.text", "clicked", 0.0);
            set_state(PART:"bg", "clicked", 0.0);
         }
      }
      program { name: "item_unclick2";
         signal: "mouse,up,1";
         source: "over2";
         script {
            set_state(PART:"elm.text", "default", 0.0);
            set_state(PART:"bg", "default", 0.0);
         }
      }
   }
}
