#: shellementary.py:878
msgid "Close"
msgstr "Zavřít"

#: shellementary.py:911
msgid "OPTIONS FOR ALL DIALOGS"
msgstr "VOLBY PRO VŠECHNY DIALOGY"

#: shellementary.py:911
msgid "These are options which you can use for all dialogs."
msgstr "Tyto volby můžete použít pro všechny dialogy."

#: shellementary.py:913
msgid "Set window title."
msgstr "Nastavit titulek okna."

#: shellementary.py:914
msgid "Set window text."
msgstr "Nastavit text okna."

#: shellementary.py:915
msgid "Set window width in pixels."
msgstr "Nastavit šířku okna v pixelech."

#: shellementary.py:916
msgid "Set window height in pixels."
msgstr "Nastavit výšku okna v pixelech."

#: shellementary.py:917
msgid "Set image as window background."
msgstr "Nastavit obrázek jako pozadí okna."

#: shellementary.py:918
msgid "Show About dialog."
msgstr "Zobrazit dialog O programu."

#: shellementary.py:921
msgid "OPTIONS TO SHOW DIALOGS"
msgstr "VOLBY KE ZOBRAZENÍ DIALOGŮ"

#: shellementary.py:921
msgid "These are options with which you can show dialogs."
msgstr "Volby ke zobrazení dialogů."

#: shellementary.py:923
msgid "Show entry dialog."
msgstr "Zobrazit dialog Pole."

#: shellementary.py:924
msgid "Show error dialog."
msgstr "Zobrazit dialog Chyba."

#: shellementary.py:925
msgid "Show warning dialog."
msgstr "Zobrazit dialog Varování."

#: shellementary.py:926
msgid "Show information dialog."
msgstr "Zobrazit dialog Informace."

#: shellementary.py:927
msgid "Show text information dialog."
msgstr "Zobrazit dialog Textová informace."

#: shellementary.py:928
msgid "Show list dialog."
msgstr "Zobrazit dialog Seznam."

#: shellementary.py:929
msgid "Show question dialog."
msgstr "Zobrazit dialog Otázka."

#: shellementary.py:930
msgid "Show clock dialog."
msgstr "Zobrazit dialog Hodiny."

#: shellementary.py:931
msgid "Show scale dialog."
msgstr "Zobrazit dialog Měřítko."

#: shellementary.py:934
msgid "Arguments which can be used only with entry dialog."
msgstr "Parametry použitelné jen pro dialog Pole."

#: shellementary.py:934
msgid "ENTRY DIALOG ARGUMENTS"
msgstr "PARAMETRY DIALOGU POLE"

#: shellementary.py:936
msgid "Set entry text"
msgstr "Nastavit text pole."

#: shellementary.py:937
msgid "Password-like entry"
msgstr "Skrýt znaky(heslo)."

#: shellementary.py:940
msgid "Arguments which can be used only with text info dialog."
msgstr "Parametry použitelné jen pri dialog Textová informace."

#: shellementary.py:940
msgid "TEXT INFO DIALOG ARGUMENTS"
msgstr "PARAMETRY DIALOGU TEXTOVÁ INFORMACE"

#: shellementary.py:942
msgid "Set file to show in text info, if you want to show text not from file, it reads from stdin."
msgstr "Zadat soubor ke zobrazení, pokud nechcete soubor, dialog čte také ze standartního vstupu."

#: shellementary.py:943
#: shellementary.py:952
msgid "Set it editable."
msgstr "Upravitelný text."

#: shellementary.py:944
msgid "Disable text wrap."
msgstr "Zakázat zalamování textu."

#: shellementary.py:947
msgid "Arguments which can be used only with clock dialog."
msgstr "Parametry použitelné jen pro dialog Hodiny."

#: shellementary.py:947
msgid "CLOCK DIALOG ARGUMENTS"
msgstr "PARAMETRY DIALOGU HODINY"

#: shellementary.py:949
msgid "Show seconds in clock."
msgstr "Zobrazit sekundy."

#: shellementary.py:950
msgid "Show AM/PM switch."
msgstr "Zobrazit přepínač Ráno/Večer."

#: shellementary.py:951
msgid "Set time in format \"HOURS, MINUTES, SECONDS\". Works in combination with --clock-editable."
msgstr "Nastavit čas ve formátu \"HODINY, MINUTY, SEKUNDY\". Funguje v kombinaci s --clock-editable."

#: shellementary.py:955
msgid "Arguments which can be used only with scale dialog."
msgstr "Parametry použitelné jen pro dialog Měřítko."

#: shellementary.py:955
msgid "SCALE DIALOG ARGUMENTS"
msgstr "PARAMETRY DIALOGU MĚŘÍTKO"

#: shellementary.py:957
msgid "Set slider value."
msgstr "Nastavit počáteční hodnotu."

#: shellementary.py:958
msgid "Set slider minimal value."
msgstr "Nastavit minimální hodnotu."

#: shellementary.py:959
msgid "Set slider maximal value."
msgstr "Nastavit maximální hodnotu."

#: shellementary.py:960
msgid "Set slider step."
msgstr "Nastavit velikost kroku."

#: shellementary.py:961
msgid "Print partial values."
msgstr "Vypsat částečné hodnoty."

#: shellementary.py:962
msgid "Hide value."
msgstr "Skrýt hodnotu."

#: shellementary.py:963
msgid "If set, slider will be inverted."
msgstr "Invertovat jezdec."

#: shellementary.py:964
msgid "Set slider unit format - for example \"value: %0.1f\", that will print value: 156.1 or %0.2f will print 156.12, %0.3f 156.125 etc."
msgstr "Nastavit formát popisku napravo - například \"hodnota: %0.1f\", vypíše hodnota: 156.1 nebo %0.2f vypíše 156.12, %0.3f 156.125 atd."

#: shellementary.py:965
msgid "Set slider label."
msgstr "Nastavit popis jezdce.(vlevo)"

#: shellementary.py:966
msgid "Set slider icon."
msgstr "Nastavit ikonu jezdce."

#: shellementary.py:967
msgid "If set, slider will be vertical."
msgstr "Svislý jezdec."

#: shellementary.py:1007
msgid "Error!"
msgstr "Chyba!"

#: shellementary.py:1017
msgid "Warning!"
msgstr "Varování!"

#: shellementary.py:1027
msgid "Information"
msgstr "Informace"

