!!!!! get modifiers right
remove Control = Mode_switch
remove Control = Shift_R
remove Shift = Menu
! Alt key -> Alt_R
keycode 108 = Alt_R
! Shift(R) key -> Shift_R
keycode 105 = Shift_R
add Shift = Shift_R
! blue key -> Mode_switch
keycode 37 = Mode_switch
add mod5 = Mode_switch

!!!!! top mute/unmute
! keycode mute = has no keycode

!!!!! side buttons
keycode 123 = Prior
keycode 122 = Next
keycode 71 = End

!!!!! top 2 buttons left/right and up from arrow key nav
keycode 69 = Home
! keycode menu = has no keycode!

!!!!! arrowkeys
keycode 111 = Up
keycode 116 = Down
keycode 113 = Left
keycode 114 = Right
keycode 36 = Return

!!!!! answer, calendar, mail, hangup/ower
! keycode answer = has no keycode!
keycode 67 = F2
keycode 68 = F3
keycode 70 = F4

