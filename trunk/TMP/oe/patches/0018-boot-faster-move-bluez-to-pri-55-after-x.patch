From: Carsten Haitzler <raster@openmoko.org>
Date: Sat, 18 Oct 2008 14:21:51 +1100
Subject: [PATCH] boot faster - move bluez to pri 55 after x

---
 packages/bluez/bluez-utils.inc  |    2 +-
 packages/bluez/bluez-utils3.inc |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/packages/bluez/bluez-utils.inc b/packages/bluez/bluez-utils.inc
index 298650a..dc1d3c2 100644
--- a/packages/bluez/bluez-utils.inc
+++ b/packages/bluez/bluez-utils.inc
@@ -33,7 +33,7 @@ EXTRA_OECONF = "--enable-initscripts --enable-bccmd   --enable-bcm203x --enable-
 inherit autotools update-rc.d
 
 INITSCRIPT_NAME = "bluetooth"
-INITSCRIPT_PARAMS = "defaults 23 19"
+INITSCRIPT_PARAMS = "defaults 55 19"
 
 do_install_append() {
 	install -d ${D}${base_sbindir} ${D}${base_bindir}/  ${D}${sysconfdir}/apm/event.d/
diff --git a/packages/bluez/bluez-utils3.inc b/packages/bluez/bluez-utils3.inc
index 0bbba2c..a6f8220 100644
--- a/packages/bluez/bluez-utils3.inc
+++ b/packages/bluez/bluez-utils3.inc
@@ -88,7 +88,7 @@ do_install_append() {
 
 
 INITSCRIPT_NAME = "bluetooth"
-INITSCRIPT_PARAMS = "defaults 23 19"
+INITSCRIPT_PARAMS = "defaults 55 19"
 
 
 PACKAGES =+ "${PN}-compat"
