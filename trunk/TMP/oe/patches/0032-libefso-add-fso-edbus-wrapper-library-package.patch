From 7ae0c796170b1cdebf706e9be993f2a96dea7b29 Mon Sep 17 00:00:00 2001
From: Carsten Haitzler (Raster) <raster@rasterman.com>
Date: Mon, 8 Dec 2008 09:31:15 +1100
Subject: [PATCH] libefso: add fso + edbus wrapper library package

---
 packages/efl1/libefso_svn.bb |   17 +++++++++++++++++
 1 files changed, 17 insertions(+), 0 deletions(-)
 create mode 100644 packages/efl1/libefso_svn.bb

diff --git a/packages/efl1/libefso_svn.bb b/packages/efl1/libefso_svn.bb
new file mode 100644
index 0000000..edec82d
--- /dev/null
+++ b/packages/efl1/libefso_svn.bb
@@ -0,0 +1,17 @@
+DESCRIPTION = "EFL FSO wrapper library"
+SECTION = "base"
+LICENSE = "GPLv2"
+DEPENDS = "ecore edbus"
+PV = "0.0.0+svnr${SRCREV}"
+PR = "r2"
+
+SRC_URI = "svn://svn.om.vptt.ch/trunk/;proto=http;module=libefso"
+S= "${WORKDIR}/libefso"
+
+inherit autotools pkgconfig
+
+do_stage () {
+        install -m 0644  src/lib/efso.h ${STAGING_INCDIR}/efso.h
+        oe_libinstall -a -so -C src/lib libefso ${STAGING_LIBDIR}
+        autotools_stage_all
+}
-- 
