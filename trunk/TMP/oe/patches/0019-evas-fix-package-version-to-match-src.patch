From d5afc21a1c1eeebffb880e26ef072cfb1a1da807 Mon Sep 17 00:00:00 2001
From: Carsten Haitzler (Raster) <raster@rasterman.com>
Date: Mon, 1 Dec 2008 10:56:23 +1100
Subject: [PATCH] evas: fix package version to match src

---
 packages/efl1/evas-native_svn.bb |    2 +-
 packages/efl1/evas.inc           |    2 +-
 packages/efl1/evas_svn.bb        |    2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/packages/efl1/evas-native_svn.bb b/packages/efl1/evas-native_svn.bb
index 3f85757..5cf72c2 100644
--- a/packages/efl1/evas-native_svn.bb
+++ b/packages/efl1/evas-native_svn.bb
@@ -1,7 +1,7 @@
 require evas.inc
 inherit native
 DEPENDS = "freetype-native libpng-native jpeg-native eet-native"
-PR = "r2"
+PR = "r1"
 
 EXTRA_OECONF = "\
 		--x-includes=${STAGING_INCDIR}/X11  \
diff --git a/packages/efl1/evas.inc b/packages/efl1/evas.inc
index f1a2660..d820979 100644
--- a/packages/efl1/evas.inc
+++ b/packages/efl1/evas.inc
@@ -2,7 +2,7 @@ DESCRIPTION = "Evas is the Enlightenment canvas API"
 LICENSE = "MIT BSD"
 # can also depend on valgrind, libsdl-x11, directfb
 DEPENDS = "eet freetype jpeg libpng virtual/libx11 libxext libxrender fontconfig"
-PV = "0.9.9.043+svnr${SRCREV}"
+PV = "0.9.9.050+svnr${SRCREV}"
 PR = "r1"
 
 
diff --git a/packages/efl1/evas_svn.bb b/packages/efl1/evas_svn.bb
index a94ae1f..6f7b379 100644
--- a/packages/efl1/evas_svn.bb
+++ b/packages/efl1/evas_svn.bb
@@ -1,5 +1,5 @@
 require evas.inc
-PR = "r3"
+PR = "r1"
 
 EXTRA_OECONF = "\
 		--x-includes=${STAGING_INCDIR}/X11  \
-- 
1.5.4.3

