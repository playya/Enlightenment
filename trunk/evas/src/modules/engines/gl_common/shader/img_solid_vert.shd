#ifdef GL_ES
precision mediump float;
#endif
attribute vec4 vertex;
attribute vec4 color;
attribute vec2 tex_coord;
attribute vec2 tex_coord2;
uniform mat4 mvp;
varying vec4 col;
varying vec2 tex_c;
varying vec2 tex_c2;
void main()
{
   gl_Position = mvp * vertex;
   col = color;
   tex_c = tex_coord;
   tex_c2 = tex_coord2;
}
