#ifdef GL_ES
#ifdef GL_FRAGMENT_PRECISION_HIGH
precision highp float;
#else
precision mediump float;
#endif
#endif
attribute vec4 vertex;
attribute vec2 tex_coord, tex_coord2, tex_coord3;
uniform mat4 mvp;
varying vec2 tex_c, tex_c2, tex_c3;
void main()
{
   gl_Position = mvp * vertex;
   tex_c = tex_coord;
   tex_c2 = tex_coord2;
   tex_c3 = tex_coord3;
}
