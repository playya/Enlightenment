               state: "default" 0.0;
               color: 0 0 0 255;
               visible: 0;
               rel1.to: "elm.text";
               rel2.to: "elm.text";
            }
            description {
               state: "focused" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part {
            name: "bg";
            type: RECT;
            description {
               state: "default" 0.0;
               color: 255 255 255 255;
               visible: 0;
               rel1 {
                  to: "outline";
                  relative: 0.0 0.0;
                  offset: 1 1;
               }
