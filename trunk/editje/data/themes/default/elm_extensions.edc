            }
         }
      }
      programs {
         program {
            name: "focused";
            signal: "elm,action,focus";
            source: "elm";
            action: STATE_SET "focused" 0.0;
            target: "line";
         }
         program {
            name: "unfocused";
            signal: "elm,action,unfocus";
            source: "elm";
            action: STATE_SET "default" 0.0;
            target: "line";
         }
      }
   }

   group {
      name: "elm/entry/selection/editje.details";
      parts {
         part {
            name: "bg";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               color: 128 128 128 128;
            }
         }
      }
   }

   group {
      name: "elm/entry/anchor/editje.details";
      parts {
         part {
            name: "bg";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               color: 128 0 0 64;
            }
         }
      }
   }

