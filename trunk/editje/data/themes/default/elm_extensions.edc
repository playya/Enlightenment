         program {
            name: "mouse_in";
            signal: "mouse,in";
            source: "over";
            action: STATE_SET "focused" 0.0;
            target: "outline";
            target: "bg";
         }
         program {
            name: "mouse_out";
            signal: "mouse,out";
            source: "over";
            action: STATE_SET "default" 0.0;
            target: "outline";
            target: "bg";
