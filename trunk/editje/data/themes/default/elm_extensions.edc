////////////////////////////////////////////////////////////////////////////
// Custom Elm Entry theme
////////////////////////////////////////////////////////////////////////////
   group {
      name: "elm/entry/base-single/editje.details";
      styles
      {
         style {
            name: "entry_single_textblock_style";
            base: "font=Sans font_size=10 align=center color=#000 wrap=none";
            tag:  "br" "\n";
         }
         style {
            name: "entry_single_textblock_style_focused";
            base: "font=Sans font_size=10 align=left color=#000 wrap=none";
            tag:  "br" "\n";
         }
         style {
            name: "entry_single_textblock_disabled_style";
            base: "font=Sans font_size=10 align=center color=#00000080" \
	      " wrap=none";
            tag:  "br" "\n";
         }
      }
      parts {
         part {
            name: "outline";
            type: RECT;
            description {
               state: "default" 0.0;
               color: 0 0 0 255;
               visible: 0;
            }
            description {
               state: "focused" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part {
            name: "bg";
            type: RECT;
            description {
               state: "default" 0.0;
               color: 255 255 255 255;
               visible: 0;
               rel1 {
                  to: "outline";
                  relative: 0.0 0.0;
                  offset: 1 1;
               }
               rel2 {
                  to: "outline";
                  relative: 1.0 1.0;
                  offset: -2 -2;
               }
            }
            description {
               state: "focused" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part {
            name: "elm.text";
            type: TEXTBLOCK;
            mouse_events: 1;
            scale: 1;
            entry_mode: EDITABLE;
            multiline: 0;
            source: "elm/entry/selection/editje.details"; // selection under
            source4: "elm/entry/cursor/editje.details"; // cursorover
            source5: "elm/entry/anchor/editje.details"; // anchor under
            description {
               state: "default" 0.0;
               text {
                  style: "entry_single_textblock_style";
                  min: 1 1;
               }
               rel1 {
                  to: "bg";
                  relative: 0.0 0.0;
               }
               rel2 {
                  to: "bg";
                  relative: 1.0 1.0;
                  offset: -1 -1;
               }
            }
            description {
               state: "disabled" 0.0;
               inherit: "default" 0.0;
               text {
                  style: "entry_single_textblock_disabled_style";
                  min: 0 1;
               }
            }
            description {
               state: "focused" 0.0;
               inherit: "default" 0.0;
               text {
                  style: "entry_single_textblock_style_focused";
               }
            }
         }
         part {
            name: "mouse_over_grabber";
            type: RECT;
            description {
               state: "default" 0.0;
               rel1.to: "outline";
               rel2.to: "outline";
               color: 0 0 0 0;
            }
            description {
               state: "focused" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
            description {
               state: "disabled" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
      }
      programs {
         program {
            name: "focus";
            signal: "load";
            source: "";
            action: FOCUS_SET;
            target: "elm.text";
         }
         program {
            name: "disable";
            signal: "elm,state,disabled";
            source: "elm";
            action: STATE_SET "disabled" 0.0;
            target: "elm.text";
            target: "mouse_over_grabber";
         }
         program {
            name: "enable";
            signal: "elm,state,enabled";
            source: "elm";
            action: STATE_SET "default" 0.0;
            target: "elm.text";
            target: "mouse_over_grabber";
         }
         program {
            name: "focused";
            signal: "elm,action,focus";
            source: "elm";
            action: STATE_SET "focused" 0.0;
            target: "elm.text";
            target: "outline";
            target: "bg";
            target: "mouse_over_grabber";
         }
         program {
            name: "unfocused";
            signal: "elm,action,unfocus";
            source: "elm";
            action: STATE_SET "default" 0.0;
            target: "elm.text";
            target: "outline";
            target: "bg";
            target: "mouse_over_grabber";
         }
         program {
            name: "mouse_in";
            signal: "mouse,in";
            source: "mouse_over_grabber";
            action: STATE_SET "focused" 0.0;
            target: "outline";
            target: "bg";
         }
         program {
            name: "mouse_out";
            signal: "mouse,out";
            source: "mouse_over_grabber";
            action: STATE_SET "default" 0.0;
            target: "outline";
            target: "bg";
         }
      }
   }

   group {
      name: "elm/entry/cursor/editje.details";
      parts {
         part {
            name: "line";
            type: RECT;
            mouse_events: 0;
            description {
