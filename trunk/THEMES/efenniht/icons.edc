   //System
   #define SYSTEM_ICON(NAME, IMAGE)                                           \
   group {                                                                    \
      name: "e/icons/"NAME ;                                                  \
      max: 87 90;																			      \
      images {                                                                \
         image: "icon_system_"IMAGE".png" COMP;                             \
         image: "icon_system_"IMAGE"_over.png" COMP;                        \
         image: "icon_system_"IMAGE"_selected.png" COMP;                    \
      }                                                                       \
      parts {                                                                 \
         part { name: "bg";                                                   \
            mouse_events: 0;                                                  \
            type: RECT;                                                       \
            description { state: "default" 0.0;                               \
               color: 0 0 0 0;																\
            }                                                                 \
         }                                                                    \
         part { name: "icon";                                                 \
            repeat_events: 1;                                                 \
            description { state: "default" 0.0;                               \
               image.normal: "icon_system_"IMAGE".png";                     \
               aspect: 1.0 1.0;                                               \
               aspect_preference: BOTH;                                       \
               rel1.to: "bg";																	\
               rel2.to: "bg";																	\
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               image.normal: "icon_system_"IMAGE"_selected.png";            \
            }                                                                 \
         }                                                                    \
         part { name: "border";                                               \
            mouse_events: 0;                                                  \
            description { state: "default" 0.0;                               \
               visible: 0;                                                    \
               color: 255 255 255 0;                                          \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               image.normal: "icon_system_"IMAGE"_over.png";                \
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               visible: 1;                                                    \
               color: 255 255 255 255;                                        \
            }                                                                 \
         }                                                                    \
         part { name: "hilight_clipper";                                      \
            mouse_events: 0;                                                  \
            type: RECT;                                                       \
            description { state: "default" 0.0;                               \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               color: 255 255 255 255;                                        \
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               visible: 0;                                                    \
               color: 255 255 255 0;                                          \
            }                                                                 \
         }                                                                    \
         part { name: "hilight";                                              \
            mouse_events: 0;                                                  \
            clip_to: "hilight_clipper";                                       \
            description { state: "default" 0.0;                               \
               visible: 0;                                                    \
               color: 255 255 255 0;                                          \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               image.normal: "icon_system_"IMAGE"_over.png";                \
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               visible: 1;                                                    \
               color: 255 255 255 255;                                        \
            }                                                                 \
            program { name: "mouse_in";                                       \
               signal: "mouse,in";                                            \
               source: "over";                                                \
               action: STATE_SET "active" 0.0;                                \
               transition: LINEAR 0.1;                                        \
               in: 0.05 0.0;                                                  \
               target: "hilight";                                             \
            }                                                                 \
            program { name: "mouse_out";                                      \
               signal: "mouse,out";                                           \
               source: "over";                                                \
               action: ACTION_STOP;                                           \
               target: "mouse_in";                                            \
               after: "mouse_out2";                                           \
            }                                                                 \
            program { name: "mouse_out2";                                     \
               action: STATE_SET "default" 0.0;                               \
               transition: LINEAR 0.2;                                        \
               target: "hilight";                                             \
            }                                                                 \
         }                                                                    \
         part { name: "over";                                                 \
            type: RECT;                                                       \
            description { state: "default" 0.0;                               \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               color: 0 0 0 0;                                                \
            }                                                                 \
         }                                                                    \
      }                                                                       \
      programs {                                                              \
         program { name: "on";                                                \
            signal: "e,state,selected";                                        \
            source: "e";                                                      \
            action: STATE_SET "active" 0.0;                                   \
            transition: LINEAR 0.2;                                           \
            target: "icon";                                                   \
            target: "border";                                                 \
            target: "hilight_clipper";                                        \
         }                                                                    \
         program { name: "off";                                               \
            signal: "e,state,unselected";                                      \
            source: "e";                                                      \
            action: STATE_SET "default" 0.0;                                  \
            transition: LINEAR 0.2;                                           \
            target: "icon";                                                   \
            target: "border";                                                 \
            target: "hilight_clipper";                                        \
         }                                                                    \
      }                                                                       \
   }                                                                          \

