collections {

   /* FreeDesktop.Org Icons
    * http://standards.freedesktop.org/icon-naming-spec/icon-naming-spec-latest.html
    *
    * In the comment you can find the old e icon name.
    *
    * Icons marked with *** are not mentioned in the spec, but we still try to
    * follow the naming convention.
    *
    * TO ADD A NEW ICON:
    * 1. Search for a name in the spec OR
    * 2. Search for a name in kde (oxygen) and gnome (tango - human) icon set OR
    * 3. Try to follow the naming convention
    */
   //Standard Action Icons
   #define ICON(NAME, FILE, SIZE)                     \
   group {                                            \
      name: "e/icons/"NAME ;                          \
      max: SIZE SIZE;                                 \
      parts {                                         \
         part {                                       \
            name: "icon";                             \
            repeat_events: 1;                        \
            description {                             \
               state: "default" 0.0;                  \
               aspect: 1.0 1.0;                       \
               aspect_preference: BOTH;               \
               image.image: FILE COMP;                \
               image.normal: FILE ;                   \
            }                                         \
         }                                            \
      }                                               \
   }



   #define DYNICON(NAME, IMAGE)                                               \
   group { name: "e/icons/"NAME ;                                             \
      images {                                                                \
         set { name: NAME;                                                    \
            image { image: "icon_"IMAGE"_16.png" COMP;                        \
               size: 1 1 16 16;                                               \
            }                                                                 \
            image { image: "icon_"IMAGE"_22.png" COMP;                        \
               size: 17 17 22 22;                                             \
            }                                                                 \
            image { image: "icon_"IMAGE"_32.png" COMP;                        \
               size: 23 23 32 32;                                             \
            }                                                                 \
            image { image: "icon_"IMAGE"_128.png" COMP;                       \
               size: 33 33 128 128;                                           \
            }                                                                 \
            image { image: "icon_"IMAGE"_256.png" COMP;                       \
               size: 129 129 256 256;                                         \
            }                                                                 \
         }                                                                    \
      }                                                                       \
      parts {                                                                 \
         part { name: "icon";                                                 \
            repeat_events: 1;                                                 \
            description { state: "default" 0.0;                               \
               aspect: 1.0 1.0;                                               \
               aspect_preference: BOTH;                                       \
               image.normal: NAME;                                            \
            }                                                                 \
         }                                                                    \
      }                                                                       \
   }

   #define SETTINGS_TOOLBAR_ICON(NAME, IMAGE)                                 \
   group {                                                                    \
      name: "e/icons/"NAME ;                                                  \
      max: 87 90;                                                             \
      images {                                                                \
         image: "icon_settings_"IMAGE".png" COMP;                             \
         image: "icon_settings_"IMAGE"_over.png" COMP;                        \
         image: "icon_settings_"IMAGE"_selected.png" COMP;                    \
      }                                                                       \
      parts {                                                                 \
         part { name: "bg";                                                   \
            mouse_events: 0;                                                  \
            description { state: "default" 0.0;                               \
               image.normal: "icon_settings_"IMAGE".png";                     \
               aspect: 1.0 1.0;                                               \
               aspect_preference: BOTH;                                       \
            }                                                                 \
         }                                                                    \
         part { name: "icon";                                                 \
            repeat_events: 1;                                                 \
            description { state: "default" 0.0;                               \
               rel1.to: "bg";						      \
               rel2.to: "bg";						      \
               color: 0 0 0 0;                                                \
               visible: 0;                                                    \
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               color: 255 255 255 255;	                                      \
               visible: 1;                                                    \
               image.normal: "icon_settings_"IMAGE"_selected.png";            \
            }                                                                 \
         }                                                                    \
         part { name: "border";                                               \
            mouse_events: 0;                                                  \
            description { state: "default" 0.0;                               \
               visible: 0;                                                    \
               color: 255 255 255 0;                                          \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               image.normal: "icon_settings_"IMAGE"_over.png";                \
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               visible: 1;                                                    \
               color: 255 255 255 255;                                        \
            }                                                                 \
         }                                                                    \
         part { name: "hilight_clipper";                                      \
            mouse_events: 0;                                                  \
            type: RECT;                                                       \
            description { state: "default" 0.0;                               \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               color: 255 255 255 255;                                        \
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               visible: 0;                                                    \
               color: 255 255 255 0;                                          \
            }                                                                 \
         }                                                                    \
         part { name: "hilight";                                              \
            mouse_events: 0;                                                  \
            clip_to: "hilight_clipper";                                       \
            description { state: "default" 0.0;                               \
               visible: 0;                                                    \
               color: 255 255 255 0;                                          \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               image.normal: "icon_settings_"IMAGE"_over.png";                \
            }                                                                 \
            description { state: "active" 0.0;                                \
               inherit: "default" 0.0;                                        \
               visible: 1;                                                    \
               color: 255 255 255 255;                                        \
            }                                                                 \
            program { name: "mouse_in";                                       \
               signal: "mouse,in";                                            \
               source: "over";                                                \
               action: STATE_SET "active" 0.0;                                \
               transition: LINEAR 0.1;                                        \
               in: 0.05 0.0;                                                  \
               target: "hilight";                                             \
            }                                                                 \
            program { name: "mouse_out";                                      \
               signal: "mouse,out";                                           \
               source: "over";                                                \
               action: ACTION_STOP;                                           \
               target: "mouse_in";                                            \
               after: "mouse_out2";                                           \
            }                                                                 \
            program { name: "mouse_out2";                                     \
               action: STATE_SET "default" 0.0;                               \
               transition: LINEAR 0.2;                                        \
               target: "hilight";                                             \
            }                                                                 \
         }                                                                    \
         part { name: "over";                                                 \
            type: RECT;                                                       \
            description { state: "default" 0.0;                               \
               rel1.to: "icon";                                               \
               rel2.to: "icon";                                               \
               color: 0 0 0 0;                                                \
            }                                                                 \
         }                                                                    \
      }                                                                       \
      programs {                                                              \
         program { name: "on";                                                \
            signal: "e,state,selected";                                        \
            source: "e";                                                      \
            action: STATE_SET "active" 0.0;                                   \
            transition: LINEAR 0.0;                                           \
            target: "icon";                                                   \
            target: "border";                                                 \
            target: "hilight_clipper";                                        \
         }                                                                    \
         program { name: "off";                                               \
            signal: "e,state,unselected";                                      \
            source: "e";                                                      \
            action: STATE_SET "default" 0.0;                                  \
            transition: LINEAR 0.0;                                           \
            target: "icon";                                                   \
            target: "border";                                                 \
            target: "hilight_clipper";                                        \
         }                                                                    \
      }                                                                       \
   }                                                                          \

   #define BUTTON_ICON(NAME, IMG, OVER)\
   part { name: "_"NAME"_button_icon_bg";                                                   \
      mouse_events: 0;                                                  \
      description { state: "default" 0.0;                               \
         image.image: "icon_"IMG".png" COMP;                     \
         image.normal: "icon_"IMG".png";                     \
         aspect: 1.0 1.0;                                               \
         aspect_preference: BOTH;                                       \
         rel1.to: OVER;						      \
         rel2.to: OVER;						      \
      }                                                                 \
      description { state: "disabled" 0.0;                               \
         inherit: "default" 0.0;                                        \
         color: 255 255 255 130;                                                \
      }                                                                 \
   }                                                                    \
   part { name: "_"NAME"_button_icon_disabler";                                      \
      mouse_events: 0;                                                  \
      type: RECT;                                                       \
      description { state: "default" 0.0;                               \
         rel1.to: "_"NAME"_button_icon_bg";                                               \
         rel2.to: "_"NAME"_button_icon_bg";                                               \
         color: 255 255 255 255;                                        \
      }                                                                 \
      description { state: "disabled" 0.0;                                \
         inherit: "default" 0.0;                                        \
         visible: 0;                                                    \
         color: 0 0 0 0;                                          \
      }                                                                 \
   }                                                                    \
   part { name: "_"NAME"_button_icon_selected";                                                   \
      mouse_events: 0;                                                  \
      clip_to: "_"NAME"_button_icon_disabler";                         \
      description { state: "default" 0.0;                               \
         rel1.to: "_"NAME"_button_icon_bg";						      \
         rel2.to: "_"NAME"_button_icon_bg";						      \
         color: 0 0 0 0;                                                \
         visible: 0;                                                    \
      }                                                                 \
      description { state: "selected" 0.0;                                \
         inherit: "default" 0.0;                                        \
         color: 255 255 255 255;	                                      \
         visible: 1;                                                    \
         image.image: "icon_"IMG"_selected.png" COMP;            \
         image.normal: "icon_"IMG"_selected.png";            \
      }                                                                 \
      program { name: "_"NAME"_button_icon_mouse_down";                                       \
         signal: "mouse,down,1";                                            \
         source: OVER;                                                \
         action: STATE_SET "selected" 0.0;                                \
         target: "_"NAME"_button_icon_selected";                                             \
      }                                                                 \
      program { name: "_"NAME"_button_icon_mouse_up";                                       \
         signal: "mouse,up,1";                                            \
         source: OVER;                                                \
         action: STATE_SET "default" 0.0;                                \
         target: "_"NAME"_button_icon_selected";                                             \
      }                                                                 \
   }                                                                    \
   part { name: "_"NAME"_button_icon_hilight";                                                   \
      mouse_events: 0;                                                  \
      clip_to: "_"NAME"_button_icon_disabler";                         \
      description { state: "default" 0.0;                               \
         visible: 0;                                                    \
         color: 255 255 255 0;                                          \
         rel1.to: "_"NAME"_button_icon_bg";                                               \
         rel2.to: "_"NAME"_button_icon_bg";                                               \
         image.image: "icon_"IMG"_over.png" COMP;                \
         image.normal: "icon_"IMG"_over.png";                \
      }                                                                 \
      description { state: "over" 0.0;                                \
         inherit: "default" 0.0;                                        \
         visible: 1;                                                    \
         color: 255 255 255 255;                                        \
      }                                                                 \
      program { name: "_"NAME"_button_icon_mouse_in";                                       \
         signal: "mouse,in";                                            \
         source: OVER;                                                \
         action: STATE_SET "over" 0.0;                                \
         transition: LINEAR 0.1;                                        \
         in: 0.05 0.0;                                                  \
         target: "_"NAME"_button_icon_hilight";                                             \
      }                                                                 \
      program { name: "_"NAME"_button_icon_mouse_out";                                      \
         signal: "mouse,out";                                           \
         source: OVER;                                                \
         action: ACTION_STOP;                                           \
         target: "_"NAME"_button_icon_mouse_in";                                            \
         after: "_"NAME"_button_icon_mouse_out2";                                           \
      }                                                                 \
      program { name: "_"NAME"_button_icon_mouse_out2";                                     \
         action: STATE_SET "default" 0.0;                               \
         transition: LINEAR 0.2;                                        \
         target: "_"NAME"_button_icon_hilight";                                             \
      }                                                                 \
   }                                                                    \

