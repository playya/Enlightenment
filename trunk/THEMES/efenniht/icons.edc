   /////////////////////////////////////////////////////////////////////////////
   /*** MOD: CONF ***/

   group { name: "e/modules/conf/main";
      images.image: "icon_conf.png" COMP;
      min: 16 16;
      max: 128 128;
      parts {
         part { name: "icon";
            mouse_events: 0;
            description { state: "default" 0.0;
               max: 16 16;
               aspect: 1.0 1.0;
               aspect_preference: BOTH;
               image.normal: "icon_conf.png";
            }
         }
         part { name: "event";
            type: RECT;
            mouse_events: 1;
            description { state: "default" 0.0;
               visible: 1;
               color: 0 0 0 0;
            }
            program { name: "button_click";
               signal: "mouse,clicked,1";
               source: "event";
               action: SIGNAL_EMIT "e,action,conf" "";
            }
         }
      }
   }
}
