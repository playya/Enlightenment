           }
           program { name: "enable_text";
               script {
                   new st[31];
                   new Float:vl;
                   get_state(PART:"elm.text", st, 30, vl);
                   if (!strcmp(st, "disabled_visible"))
                   set_state(PART:"elm.text", "visible", 0.0);
                   else
                   set_state(PART:"elm.text", "default", 0.0);
               }
           }
       }
