                   new v;
                   v = get_int(sbvis_v);
                   v |= get_int(sbalways_v);
                   if (!v) {
                       set_int(sbalways_v, 1);
                       emit("do-show-vbar", "");
                       set_int(sbvis_v, 1);
                   }
               }
