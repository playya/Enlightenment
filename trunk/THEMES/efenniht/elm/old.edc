           program { name: "go2";
               signal: "elm,state,busy,start";
               source: "elm";
               action: STATE_SET "default" 0.0;
               transition: LINEAR 0.5;
               target: "busy";
               after:  "go2";
           }
           program { name: "stop1";
               signal: "elm,state,busy,stop";
               source: "elm";
               action: STATE_SET "default" 0.0;
               transition: SINUSOIDAL 1.0;
               target: "busy_clip";
               after: "stop2";
           }
         program { name: "stop2";
            action: ACTION_STOP;
            target: "go2";
         }
      }
   }

