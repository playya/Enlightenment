collections {
   group { name: "elm/button/base/hoversel_vertical/default";
      alias: "elm/button/base/hoversel_vertical/entry";
      alias: "elm/button/base/hoversel_horizontal/default";
      alias: "elm/button/base/hoversel_horizontal/entry";
      images {
         image: "button_side_line.png" COMP;
         image: "button_bg_normal.png" COMP;
         image: "button_bg_disabled.png" COMP;
         image: "shelf_hilight.png" COMP;
         image: "window_bt_down.png" COMP;
      }
      script {
         public icon_visible;
         public label_visible;
         public disabled;
      }
      parts {
         part { name: "button_bg";
            mouse_events: 0;
            description { state: "default" 0.0;
               min: 32 24;
               image {
                  normal: "button_bg_normal.png";
                  border: 0 0 4 4;
               }
               rel1.offset: 2 2;
               rel2.offset: -3 -3;
            }
            description { state: "disabled" 0.0;
               inherit: "default" 0.0;
            }
         }
         part { name: "button_left";
            mouse_events: 0;
            description { state: "default" 0.0;
               state: "default" 0.0;
               rel1 {
                  relative: 0.0 0.0;
                  offset: 2 0;
               }
               rel2 {
                  relative: 0.0 1.0;
                  offset: 2 -3;
               }
               image.normal: "button_side_line.png";
            }
         }
         part { name: "button_right";
            mouse_events: 0;
            description { state: "default" 0.0;
               state: "default" 0.0;
               rel1 {
                  relative: 1.0 0.0;
                  offset: -3 0;
               }
               rel2 {
                  relative: 1.0 1.0;
                  offset: -3 -3;
               }
               image.normal: "button_side_line.png";
            }
         }
         part { name: "button_down";
            mouse_events: 0;
            description { state: "default" 0.0;
               image.normal: "window_bt_down.png";
               rel1.to: "button_bg";
               rel1.offset: 0 3;
               rel2.to: "button_bg";
               rel2.offset: -1 -4;
               visible: 0;
               color: 255 255 255 0;
            }
            description { state: "down" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
            program { name: "button_down";
               signal: "mouse,down,1";
               source: "event";
               action: STATE_SET "down" 0.0;
               target: "button_down";
            }
            program { name: "button_up";
               signal: "mouse,up,1";
               source: "event";
               action: STATE_SET "default" 0.0;
               target: "button_down";
            }
         }
         part { name: "button_hover";
            mouse_events: 0;
            description { state: "default" 0.0;
               image.normal: "shelf_hilight.png";
               image.border: 0 0 1 1;
               rel1.to: "button_bg";
               rel1.offset: 0 3;
               rel2.to: "button_bg";
               rel2.offset: -1 -4;
               visible: 0;
               color: 255 255 255 0;
            }
            description { state: "hover" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
            program { name: "button_in";
               signal: "mouse,in";
               source: "event";
               action: STATE_SET "hover" 0.0;
               in: 0.05 0.0;
               transition: LINEAR 0.2;
               target: "button_hover";
               target: "elm.text_hilight";
            }
            program { name: "button_out";
               signal: "mouse,out";
               source: "event";
               action: ACTION_STOP;
               target: "button_in";
               after: "button_out2";
            }
            program { name: "button_out2";
               action: STATE_SET "default" 0.0;
               transition: LINEAR 0.2;
               target: "button_hover";
               target: "elm.text_hilight";
            }
         }
         part { name: "elm.swallow.content";
            type: SWALLOW;
            description { state: "default" 0.0;
               fixed: 1 0;
               visible: 0;
               align: 0.0 0.5;
               rel1.offset: 4 6;
               rel2.offset: 4 -7;
               rel2.relative: 0.0 1.0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               fixed: 1 0;
               visible: 1;
               aspect: 1.0 1.0;
               aspect_preference: VERTICAL;
            }
            description { state: "icononly" 0.0;
               inherit: "visible" 0.0;
               fixed: 0 0;
               align: 0.5 0.5;
               rel2.offset: -5 -7;
               rel2.relative: 1.0 1.0;
            }
         }
         part { name: "elm.text";
            type: TEXT;
            mouse_events: 0;
            scale: 1;
            description { state: "default" 0.0;
               visible: 0;
               rel1 {
                  to_x: "elm.swallow.content";
                  to_y: "button_bg";
                  relative: 1.0 0.0;
                  offset: 0 4;
               }
               rel2 {
                  to: "button_bg";
                  relative: 1.0 1.0;
                  offset: -5 -5;
               }
               align: 0.0 0.5;
               color_class: "button_text";
               color: 255 255 255 0;
               text {
                  font: "DejaVu Sans:style=Bold,Sans:style=Bold";
                  size: 10;
                  min: 0 0;
                  align: 0.5 0.5;
               }
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
               text.min: 1 1;
            }
            description { state: "disabled_visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
               text.min: 1 1;
               color_class: "button_text_disabled";
            }
         }
         part { name: "elm.text_hilight";
            type: TEXT;
            mouse_events: 0;
            scale: 1;
            description { state: "default" 0.0;
               visible: 0;
               rel1.to: "elm.text";
               rel2.to: "elm.text";
               color_class: "button_text_over";
               color: 255 255 255 0;
               text {
                  font: "DejaVu Sans:style=Bold,Sans:style=Bold";
                  size: 10;
                  min: 0 0;
                  align: 0.5 0.5;
                  text_source: "elm.text";
               }
            }
            description { state: "hover" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
         }
         part { name: "event";
            type: RECT;
            ignore_flags: ON_HOLD;
            description { state: "default" 0.0;
               rel1.to: "button_bg";
               rel2.to: "button_bg";
               color: 0 0 0 0;
            }
            description { state: "disabled" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
      }
      programs {
         program {
            name:   "button_click";
            signal: "mouse,clicked,1";
            source: "event";
            action: SIGNAL_EMIT "elm,action,click" "";
         }
         program { name: "text_show";
            signal: "elm,state,text,visible";
            source: "elm";
            script {
               set_int(label_visible, 1);
               if (get_int(icon_visible))
               set_state(PART:"elm.swallow.content", "visible", 0.0);
               if (get_int(disabled))
               set_state(PART:"elm.text", "disabled_visible", 0.0);
               else
               set_state(PART:"elm.text", "visible", 0.0);
            }
         }
         program { name: "text_hide";
            signal: "elm,state,text,hidden";
            source: "elm";
            script {
               set_int(label_visible, 0);
               set_state(PART:"elm.text", "default", 0.0);
               if (get_int(icon_visible))
               set_state(PART:"elm.swallow.content", "icononly", 0.0);
            }
         }
         program { name: "icon_show";
            signal: "elm,state,icon,visible";
            source: "elm";
            script {
               set_int(icon_visible, 1);
               if (get_int(label_visible))
               set_state(PART:"elm.swallow.content", "visible", 0.0);
               else
               set_state(PART:"elm.swallow.content", "icononly", 0.0);
            }
         }
         program { name: "icon_hide";
            signal: "elm,state,icon,hidden";
            source: "elm";
            script {
               set_int(icon_visible, 0);
               set_state(PART:"elm.swallow.content", "default", 0.0);
            }
         }
         program { name: "disable";
            signal: "elm,state,disabled";
            source: "elm";
            action: STATE_SET "disabled" 0.0;
            target: "event";
            target: "button_bg";
            after: "disable_text";
         }
         program { name: "disable_text";
            script {
               set_int(disabled, 1);
               if (get_int(label_visible))
               set_state(PART:"elm.text", "disabled_visible", 0.0);
            }
         }
         program { name: "enable";
            signal: "elm,state,enabled";
            source: "elm";
            action: STATE_SET "default" 0.0;
            target: "event";
            target: "button_bg";
            after: "enable_text";
         }
         program { name: "enable_text";
            script {
               set_int(disabled, 0);
               if (get_int(label_visible))
               set_state(PART:"elm.text", "visible", 0.0);
            }
         }
      }
   }
   group { name: "elm/button/base/hoversel_vertical_entry/default";
      alias: "elm/button/base/hoversel_vertical_entry/entry";
      alias: "elm/button/base/hoversel_horizontal_entry/default";
      alias: "elm/button/base/hoversel_horizontal_entry/entry";
      images {
         image: "list_item_selected_bg.png" COMP;
      }
      script {
         public icon_visible;
         public label_visible;
      }
      parts {
         part { name: "bg";
            mouse_events: 0;
            description { state: "default" 0.0;
               visible: 0;
               color: 255 255 255 0;
               image {
                  normal: "list_item_selected_bg.png";
                  border: 0 0 4 4;
               }
               fill.smooth: 1;
            }
            description { state: "selected" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
         }
         part { name: "elm.swallow.content";
            type: SWALLOW;
            description { state: "default" 0.0;
               visible: 0;
               align: 0.0 0.5;
               rel1.offset: 4 4;
               rel2.offset: 3 -5;
               rel2.relative: 0.0 1.0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               aspect: 1.0 1.0;
               aspect_preference: VERTICAL;
               rel2.offset: 4 -5;
            }
            description { state: "icononly" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               align: 0.5 0.5;
               aspect: 1.0 1.0;
               rel2.offset: -5 -5;
               rel2.relative: 1.0 1.0;
               aspect_preference: VERTICAL;
            }
         }
         part { name: "text_clipper";
            type: RECT;
            mouse_events: 1;
            description { state: "default" 0.0;
            }
            description { state: "icononly" 0.0;
               inherit: "default" 0.0;
               visible: 0;
               color: 0 0 0 0;
            }
         }
         part {
            name:          "elm.text";
            type:          TEXT;
            effect:        SOFT_SHADOW;
            mouse_events:  0;
            scale: 1;
            clip_to: "text_clipper";
            description { state: "default" 0.0;
               rel1.to_x: "elm.swallow.content";
               rel1.relative: 1.0 0.0;
               rel1.offset: 0 4;
               rel2.offset: -5 -5;
               color_class: "list_item";
               text {
                  font: "DejaVu Sans,Sans";
                  size: 10;
                  min: 1 1;
                  align: 0.5 0.5;
                  text_class: "ilist_item";
               }
            }
            description { state: "selected" 0.0;
               inherit: "default" 0.0;
               text.min: 1 1;
               color_class: "list_item_selected";
            }
         }
         part { name: "event";
            type: RECT;
            repeat_events: 0;
            description {
               state: "default" 0.0;
               color: 0 0 0 0;
            }
         }
      }
      programs {
         program {
            name:   "button_down";
            signal: "mouse,down,1";
            source: "event";
            action: STATE_SET "selected" 0.0;
            target: "bg";
            target: "elm.text";
         }
         program {
            name:   "button_up";
            signal: "mouse,up,1";
            source: "event";
            action: STATE_SET "default" 0.0;
            target: "bg";
            target: "elm.text";
         }
         program {
            name:   "button_click";
            signal: "mouse,clicked,1";
            source: "event";
            action: SIGNAL_EMIT "elm,action,click" "";
         }
         program { name: "text_show";
            signal: "elm,state,text,visible";
            source: "elm";
            script {
               set_int(label_visible, 1);
               if (get_int(icon_visible))
               set_state(PART:"elm.swallow.content", "visible", 0.0);
               set_state(PART:"text_clipper", "default", 0.0);
            }
         }
         program { name: "text_hide";
            signal: "elm,state,text,hidden";
            source: "elm";
            script {
               set_int(label_visible, 0);
               set_state(PART:"elm.text", "default", 0.0);
               if (get_int(icon_visible))
               set_state(PART:"elm.swallow.content", "icononly", 0.0);
               set_state(PART:"text_clipper", "icononly", 0.0);
            }
         }
         program { name: "icon_show";
            signal: "elm,state,icon,visible";
            source: "elm";
            script {
               set_int(icon_visible, 1);
               if (get_int(label_visible))
               set_state(PART:"elm.swallow.content", "visible", 0.0);
               else
               set_state(PART:"elm.swallow.content", "icononly", 0.0);
            }
         }
         program { name: "icon_hide";
            signal: "elm,state,icon,hidden";
            source: "elm";
            script {
               set_int(icon_visible, 0);
               set_state(PART:"elm.swallow.content", "default", 0.0);
            }
         }
      }
   }

   group { name: "elm/hover/base/hoversel_vertical/default";
      alias: "elm/hover/base/hoversel_vertical/entry";
      parts {
         part { name: "elm.swallow.offset";
            type: SWALLOW;
            description { state: "default" 0.0;
               align: 0.0 0.0;
               rel1.relative: 0.0 0.0;
               rel2.relative: 0.0 0.0;
            }
         }
         part { name: "elm.swallow.size";
            type: SWALLOW;
            description { state: "default" 0.0;
               align: 0.0 0.0;
               rel1.to: "elm.swallow.offset";
               rel1.relative: 1.0 1.0;
               rel2.to: "elm.swallow.offset";
               rel2.relative: 1.0 1.0;
            }
         }

         part { name: "base";
            type: RECT;
            mouse_events: 1;
            description { state: "default" 0.0;
               color: 0 0 0 0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 1.0;
               color: 0 0 0 150;
            }
         }

         part { name: "bg";
            mouse_events: 1;
            type: RECT;
            description { state: "default" 0.0;
               rel1.to_x: "elm.swallow.slot.top";
               rel1.to_y: "elm.swallow.slot.top";
               rel1.offset: -2 -6;
               rel2.to_x: "elm.swallow.slot.top";
               rel2.to_y: "elm.swallow.slot.bottom";
               rel2.offset: 1 5;
               color: 255 0 255 100;
            }
            description { state: "bottom" 0.0;
               rel1.to_x: "elm.swallow.slot.bottom";
               rel1.to_y: "elm.swallow.slot.top";
               rel1.offset: -2 -6;
               rel2.to_x: "elm.swallow.slot.bottom";
               rel2.to_y: "elm.swallow.slot.bottom";
               rel2.offset: 1 5;
               color: 255 0 255 100;
            }
         }
         BG_BOX("bg", 1);

         part { name: "topclip";
            type: RECT;
            description { state: "default" 0.0;
               rel2.to_y: "edge_top";
               rel2.relative: 1.0 0.0;
               rel2.offset: -1 7;
            }
         }
         part { name: "elm.swallow.slot.top";
            type: SWALLOW;
            clip_to: "topclip";
            description { state: "default" 0.0;
               visible: 1;
               align: 0.5 0.0;
               rel1.to: "elm.swallow.slot.middle";
               rel1.relative: 0.0 0.0;
               rel1.offset: 0 -1;
               rel2.to: "elm.swallow.slot.middle";
               rel2.relative: 1.0 0.0;
               rel2.offset: -1 -1;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               rel1.offset: 0 -7;
               rel2.offset: -1 -7;
               align: 0.5 1.0;
            }
         }

         part { name: "bottomclip";
            type: RECT;
            description { state: "default" 0.0;
               rel1.to_y: "edge_bottom";
               rel1.relative: 0.0 1.0;
               rel1.offset: -1 -8;
            }
         }
         part { name: "elm.swallow.slot.bottom";
            type: SWALLOW;
            clip_to: "bottomclip";
            description { state: "default" 0.0;
               align: 0.5 1.0;
               rel1.to: "elm.swallow.slot.middle";
               rel1.relative: 0.0 1.0;
               rel1.offset: 0 0;
               rel2.to: "elm.swallow.slot.middle";
               rel2.relative: 1.0 1.0;
               rel2.offset: -1 0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               rel1.offset: 0 6;
               rel2.offset: -1 6;
               align: 0.5 0.0;
            }
         }

         part { name: "edge_top";
            type: RECT;
            mouse_events:  0;
            description { state: "default" 0.0;
               visible: 0;
               rel1 {
                  to: "elm.swallow.size";
                  offset: 0 -10;
               }
               rel2 {
                  to: "elm.swallow.size";
               }
               color: 255 0 0 100;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part { name: "edge_bottom";
            mouse_events:  0;
            type: RECT;
            description { state: "default" 0.0;
               visible: 0;
               rel1 {
                  to: "elm.swallow.size";
               }
               rel2 {
                  to: "elm.swallow.size";
                  offset: -1 9;
               }
               color: 0 255 0 100;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part { name: "elm.swallow.slot.middle";
            type: SWALLOW;
            description { state: "default" 0.0;
               rel1.to: "elm.swallow.size";
               rel2.to: "elm.swallow.size";
            }
         }
      }
      programs {
         program { name: "end";
            signal: "mouse,up,1";
            source: "base";
            action: SIGNAL_EMIT "elm,action,dismiss" "";
         }

         program { name: "show";
            signal: "elm,action,show";
            source: "elm";
            action: STATE_SET "visible" 0.0;
                    //	    transition: DECELERATE 0.5;
            target: "base";
            target: "bg";
         }
         program { name: "hide";
            signal: "elm,action,hide";
            source: "elm";
            action: STATE_SET "default" 0.0;
                    //	    transition: DECELERATE 0.5;
            target: "base";
            target: "bg";
         }
         program { name: "topshow";
            signal: "elm,action,slot,top,show";
            source: "elm";
            action: STATE_SET "visible" 0.0;
            target: "edge_top";
            after: "topshow2";
         }
         program { name: "topshow2";
            action: STATE_SET "visible" 0.0;
            transition: DECELERATE 0.5;
            target: "elm.swallow.slot.top";
         }
         program { name: "topshow3";
            signal: "elm,action,slot,top,show";
            source: "elm";
            action: STATE_SET "default" 0.0;
            target: "bg";
         }
         program { name: "tophide";
            signal: "elm,action,slot,top,hide";
            source: "elm";
            action: STATE_SET "default" 0.0;
            transition: DECELERATE 0.5;
            target: "elm.swallow.slot.top";
            after: "tophide2";
         }
         program { name: "tophide2";
            action: STATE_SET "default" 0.0;
            target: "edge_top";
         }
         program { name: "bottomshow";
            signal: "elm,action,slot,bottom,show";
            source: "elm";
            action: STATE_SET "visible" 0.0;
            target: "edge_bottom";
            after: "bottomshow2";
         }
         program { name: "bottomshow2";
            action: STATE_SET "visible" 0.0;
            transition: DECELERATE 0.5;
            target: "elm.swallow.slot.bottom";
         }
         program { name: "bottomshow3";
            signal: "elm,action,slot,bottom,show";
            source: "elm";
            action: STATE_SET "bottom" 0.0;
            target: "bg";
         }
         program { name: "bottomhide";
            signal: "elm,action,slot,bottom,hide";
            source: "elm";
            action: STATE_SET "default" 0.0;
            transition: DECELERATE 0.5;
            target: "elm.swallow.slot.bottom";
            after: "bottomhide2";
         }
         program { name: "bottomhide2";
            action: STATE_SET "default" 0.0;
            target: "edge_bottom";
         }
      }
   }
}

