/*** MOD: ILLUME-SOFTKEY ***/
group { name: "modules/illume-softkey/window";
   parts {
      SHELF_BASE
      part { name: "e.box.extra_buttons";
         type: BOX;
         description { state: "default" 0.0;
            rel1 {
               offset: 3 0;
               to: "base";
            }
            rel2 {
               relative: 0.60 1.0;
               offset: 0 -1;
               to: "base";
            }
            box {
               layout: "horizontal";
               padding: 0 0;
               align: 0.0 0.5;
               min: 1 1;
            }
         }
      }
      part { name: "e.box.buttons";
         type: BOX;
         description { state: "default" 0.0;
            rel1 {
               relative: 1.0 0.0;
               to: "e.box.extra_buttons";
            }
            rel2 {
               offset: -2 -1;
               to_x: "base";
               to_y: "e.box.extra_buttons";
            }
            box {
               layout: "horizontal";
               padding: 2 0;
               align: 1.0 0.5;
               min: 1 1;
            }
         }
      }
   }
}
