/////////////////////////////////////////////////////////////////////////////
/*** MOD: ILLUME-HOME-TOGGLE ***/

group { name: "e/modules/illume_home_toggle/main";
   images.image: "icon_illume_home.png" COMP;
   min: 16 16;
   max: 128 128;
   parts {
      part { name: "icon";
         mouse_events: 0;
         description { state: "default" 0.0;
            max: 15 15;
            aspect: 1.0 1.0;
            aspect_preference: BOTH;
            image.normal: "icon_illume_home.png";
         }
      }
      part { name: "event";
         type: RECT;
         mouse_events: 1;
         description { state: "default" 0.0;
            visible: 1;
            color: 0 0 0 0;
         }
         program { name: "button_click";
            signal: "mouse,clicked,1";
            source: "event";
            action: SIGNAL_EMIT "e,action,home" "";
         }
      }
   }
}

/////////////////////////////////////////////////////////////////////////////
/*** MOD: ILLUME-KBD-TOGGLE ***/

group { name: "e/modules/illume_kbd_toggle/main";
   images.image: "icon_illume_kbd.png" COMP;
   images.image: "icon_illume_kbd_on.png" COMP;
   min: 16 16;
   max: 128 128;
   parts {
      part {
         name: "icon";
         mouse_events: 0;
         description {
            state: "default" 0.0;
            aspect: 1.0 1.0;
            aspect_preference: BOTH;
            image.normal: "icon_illume_kbd.png";
            max: 32 32;
         }
         description { state: "on" 0.0;
            inherit: "default" 0.0;
            image.normal: "icon_illume_kbd_on.png";
         }
         program { name: "state_on";
            signal: "e,state,vkbd,on";
            source: "e";
            action: STATE_SET "on" 0.0;
            target: "icon";
            target: "on";
         }
         program { name: "state_off";
            signal: "e,state,vkbd,off";
            source: "e";
            action: STATE_SET "default" 0.0;
            target: "icon";
            target: "on";
         }
      }
      part { name: "event";
         type: RECT;
         mouse_events: 1;
         description { state: "default" 0.0;
            visible: 1;
            color: 0 0 0 0;
         }
         program { name: "enable";
            signal: "mouse,clicked,1";
            source: "event";
            action: SIGNAL_EMIT "e,action,vkbd,enable" "";
         }
      }
      part { name: "on";
         type: RECT;
         mouse_events: 1;
         description { state: "default" 0.0;
            visible: 0;
            color: 0 0 0 0;
         }
         description { state: "on" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
         program { name: "disable";
            signal: "mouse,clicked,1";
            source: "on";
            action: SIGNAL_EMIT "e,action,vkbd,disable" "";
         }
      }
   }
}

/////////////////////////////////////////////////////////////////////////////
/*** MOD: ILLUME-MODE-TOGGLE ***/

group {
   name: "e/modules/illume_mode_toggle/main";
   images.image: "icon_illume_mode_single.png" COMP;
   images.image: "icon_illume_mode_dual_left.png" COMP;
   images.image: "icon_illume_mode_dual_top.png" COMP;
   min: 16 16;
   max: 128 128;
   parts {
      part {
         name: "icon";
         mouse_events: 0;
         description {
            state: "default" 0.0;
            aspect: 1.0 1.0;
            aspect_preference: BOTH;
            image.normal: "icon_illume_mode_single.png";
            max: 15 15;
         }
         description { state: "dual_left" 0.0;
            inherit: "default" 0.0;
            image.normal: "icon_illume_mode_dual_left.png";
         }
         description { state: "dual_top" 0.0;
            inherit: "default" 0.0;
            image.normal: "icon_illume_mode_dual_top.png";
         }
         program { name: "state_single";
            signal: "e,mode,single";
            source: "e";
            action: STATE_SET "default" 0.0;
            target: "icon";
            target: "dual_left";
            target: "dual_top";
         }
         program { name: "state_dual_left";
            signal: "e,mode,dual,left";
            source: "e";
            action: STATE_SET "dual_left" 0.0;
            target: "icon";
            target: "dual_left";
            target: "dual_top";
         }
         program { name: "state_dual_top";
            signal: "e,mode,dual,top";
            source: "e";
            action: STATE_SET "dual_top" 0.0;
            target: "icon";
            target: "dual_left";
            target: "dual_top";
         }
      }
      part { name: "single";
         type: RECT;
         mouse_events: 1;
         description { state: "default" 0.0;
            visible: 1;
            color: 0 0 0 0;
         }
         program { name: "single";
            signal: "mouse,clicked,1";
            source: "single";
            action: SIGNAL_EMIT "e,action,mode,dual,top" "";
         }
      }
      part { name: "dual_top";
         type: RECT;
         mouse_events: 1;
         description { state: "default" 0.0;
            visible: 0;
            color: 0 0 0 0;
         }
         description { state: "dual_top" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
         description { state: "dual_left" 0.0;
            inherit: "default" 0.0;
            visible: 0;
         }
         program { name: "dual_top";
            signal: "mouse,clicked,1";
            source: "dual_top";
            action: SIGNAL_EMIT "e,action,mode,dual,left" "";
         }
      }
      part { name: "dual_left";
         type: RECT;
         mouse_events: 1;
         description { state: "default" 0.0;
            visible: 0;
            color: 0 0 0 0;
         }
         description { state: "dual_top" 0.0;
            inherit: "default" 0.0;
            visible: 0;
         }
         description { state: "dual_left" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
         program { name: "dual_left";
            signal: "mouse,clicked,1";
            source: "dual_left";
            action: SIGNAL_EMIT "e,action,mode,single" "";
         }
      }
   }
}


