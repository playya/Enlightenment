               visible: 0;
               rel1 {
                  relative: 0.0 0.0;
                  to: "base";
               }
               rel2 {
                  relative: 1.0 0.0;
                  offset: -1  0;
                  to_y: "bar_base";
                  to_x: "base";
               }
            }
         }

         BG_GRADIENT("gradient")

         part {
            name: "center";
            type: RECT;
            description {
               state: "default" 0.0;
               min: 1 1;
               max: 1 1;
               visible: 0;
               rel1.to: "e.swallow.list";
               rel1.relative: 0.5 0.5; 
               rel1.to: "e.swallow.list";
               rel2.relative: 0.5 0.5; 
               /* align: 0.5 0.5; */
            }
         }
         part {
            name: "center2";
            type: RECT;
            description {
               state: "default" 0.0;
               min: 1 1;
               max: 1 1;
               visible: 0;
               rel1.to: "e.swallow.list2";
               rel1.relative: 0.5 0.5; 
               rel1.to: "e.swallow.list2";
               rel2.relative: 0.5 0.5; 
               /* align: 0.5 0.5; */
            }
         }

         part {
            name: "e.swallow.list";
            type: SWALLOW;
            clip_to: "list_clip";
            mouse_events: 1;
            description {
               state: "default" 0.0;
               min: 32 170;
               fixed: 1 1;
               rel1 {
                  relative: 0.0 0.0;
                  offset: 0 3;
                  to: "list_clip";
               }
               rel2 {
                  relative: 1.0 1.0;
                  offset: -1  -1;
                  to: "list_clip";
               }
            }
            description {
               state: "map" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
               #ifdef ROTATE
               map {
                  on: 1;
                  perspective_on: 1;
                  rotation.y: 0.0;
               }
               #endif
            }
            description {
               state: "default" 1.0;
               inherit: "default" 0.0;
               visible: 0;

               rel1 {
                  offset: 0 3;
                  to: "list_clip";

               }
               rel2 {
                  offset: -1  -1;
                  to: "list_clip";
               }
               #ifdef SLIDE
               rel1.relative: -0.5 0.0;
               rel2.relative: 0.5 1.0;
               #endif
               map {
                  on: 0;
                  perspective_on: 0;
               }
            }
            description {
               state: "map" 1.0;
               inherit: "default" 1.0;
               color: 255 255 255 0;
               visible: 1;
               #ifdef ROTATE
               map {
                  on: 1;
                  perspective_on: 1;
                  rotation.y: -ANGLE;
               }
               #endif
            }
         }
         part {
            name: "e.swallow.list2";
            type: SWALLOW;
            clip_to: "list_clip";
            mouse_events: 1;
            description {
               state: "default" 0.0;
               visible: 0;
               min: 32 170;
               fixed: 1 1;

               rel1 {
                  offset: 0 3;
                  to: "list_clip";
               }
               rel2 {
                  offset: -1  -1;
                  to: "list_clip";
               }
               #ifdef SLIDE
               rel1.relative: 0.5 0.0;
               rel2.relative: 1.5 1.0;
               #endif
            }
            description {
               state: "map" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 0;
               #ifdef ROTATE
               map {
                  on: 1;
                  perspective_on: 1;
                  rotation.center: "center2";
                  rotation.y: ANGLE;
               }
               #endif
            }
            description {
               state: "default" 1.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
               rel1 {
                  offset: 0 3;
                  to: "list_clip";
               }
               rel2 {
                  offset: -1  -1;
                  to: "list_clip";
               }
               #ifdef SLIDE
               rel1.relative: 0.0 0.0;
               rel2.relative: 1.0 1.0;
               #endif
               map {
                  on: 0;
                  perspective_on: 0;
               }
            }
            description {
               state: "map" 1.0;
               inherit: "default" 1.0;
               color: 255 255 255 255;
               #ifdef ROTATE
               map {
                  on: 1;
                  perspective_on: 1;
                  rotation.center: "center";
                  rotation.y: 0.0;
               }
               #endif
            }
         }
