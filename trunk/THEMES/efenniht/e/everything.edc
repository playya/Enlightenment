                  relative: 1.0 0.0;
                  to_x: "base";
                  offset: -1 1;
               }
            }
            description {
               state: "visible" 0.0;
               inherit: "default" 0.0;
               rel2.offset: -1 28;
            }
         }
         part {
            name: "bar_base";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               visible: 0;
               rel1 {
                  relative: 0.0 1.0;
                  to: "base";
                  offset: 0 -28;
               }
               rel2 {
                  relative: 1.0 1.0;
                  to: "base";
                  offset: -1 -1;
               }
            }
         }
         part {
            name: "bar_clip";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               rel1 {
