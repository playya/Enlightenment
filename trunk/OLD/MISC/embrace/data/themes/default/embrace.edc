/* $Id$ */

group {
	min, 100 60;
	max, 5000 5000;
	name, "Embrace";

	data {
		item, "trans_bg" "1";
	}

	parts {
		part {
			name, "background";
			type, RECT;
			repeat_events, true;

			description {
				state, "default" 0.0;
				color, 0 0 0 64;
			}
		}

		part {
			name, "Container";
			type, RECT;
			repeat_events, true;

			description {
				state, "default" 0.0;
				color, 0 0 0 64;

				rel1 {offset, 5 5;}
				rel2 {offset, -5 -5;}
			}
		}
	}
}
