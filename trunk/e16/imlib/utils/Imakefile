#include "../cf/ImlibPre.tmpl"

NAME= imlib_config

SRCS= imlib_config.c testimg.c icons.c
OBJS= imlib_config.o testimg.o icons.o
 
AllTarget($(NAME))

DEFINES = $(ALL_DEFINES)

INCLUDES = -I../gdk_imlib $(ALL_INCLUDES)

SYS_LIBRARIES = $(XLIB) -lm

LOCAL_LIBRARIES = -lgdk_imlib $(GRFX_LIB) -lgdk -lgtk -lglib

LOCAL_LDFLAGS = -L../gdk_imlib $(ALL_LOCAL_LDFLAGS)

NormalProgramTarget($(NAME),$(OBJS),,$(LOCAL_LIBRARIES),)

DependTarget()
LintTarget()

#include "../cf/ImlibPost.tmpl"
