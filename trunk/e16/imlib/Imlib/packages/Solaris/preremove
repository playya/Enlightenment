#!/bin/sh
mv /etc/imrc /etc/imrc.old > /dev/null 2>&1
mv /usr/local/etc/imrc /usr/local/etc/imrc.old > /dev/null 2>&

mv /etc/im_palette.pal /etc/im_palette.pal.old > /dev/null 2>&
mv /usr/local/etc/im_palette.pal /usr/local/etc/im_palette.pal.old > /dev/null 2>&

( rm -rf $BASEDIR/lib/libImlib.so > /dev/null 2>&1 )

exit 0
