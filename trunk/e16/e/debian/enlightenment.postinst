#!/bin/sh -e

wm=enlightenment


if test -x /usr/sbin/update-alternatives; then /usr/sbin/update-alternatives  --install /usr/bin/x-window-manager x-window-manager /usr/bin/$wm 50 --slave /usr/share/man/man1/x-window-manager.1.gz x-window-manager.1.gz /usr/share/man/man1/$wm.1.gz; fi

if test -x /usr/sbin/wm-menu-config; then /usr/sbin/wm-menu-config $wm on;fi

