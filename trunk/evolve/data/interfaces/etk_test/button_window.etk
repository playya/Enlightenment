/* Button test window */
widget
{
   type: "window";
   name: "/window.buttons";
   title: "Button Test";
}

widget
{
   type: "table";
   name: "/window.buttons/table";
   parent: "/window.buttons";
   num-cols: 2;
   num-rows: 2;
}

widget
{
   type: "frame";
   name: "/window.buttons/table/frame";
   parent: "/window.buttons/table";
   label: "Buttons";
   packing
   {
      left_attach: 0;
      right_attach: 0;
      top_attach: 0;
      bottom_attach: 0;
   }
}

widget
{
   type: "vbox";
   name: "/window.buttons/table/frame/vbox";
   parent: "/window.buttons/table/frame";
}

widget
{  
   type: "button";
   name: "/window.buttons/table/frame/vbox/normal_button";
   parent: "/window.buttons/table/frame/vbox";
   label: "Normal buttom";
   signal
   {
      name: "clicked";
      emit: "custom_signal_emission";
   }
}

widget
{  
   type: "button";
   name: "/window.buttons/table/frame/vbox/image_button";
   parent: "/window.buttons/table/frame/vbox";
   label: "Button with an image";
}

widget
{  
   type: "button";
   name: "/window.buttons/table/frame/vbox/empty_button";
   parent: "/window.buttons/table/frame/vbox";
}

widget
{  
   type: "button";
   name: "/window.buttons/table/frame/vbox/disabled_button";
   parent: "/window.buttons/table/frame/vbox";
   label: "Disabled button";
   disabled: 1;
}

widget
{
   type: "frame";
   name: "/window.buttons/table/frame_toggle";
   parent: "/window.buttons/table";
   label: "Toggle Buttons";
   packing
   {
      left_attach: 1;
      right_attach: 1;
      top_attach: 0;
      bottom_attach: 0;
   }
}

widget
{
   type: "vbox";
   name: "/window.buttons/table/frame_toggle/vbox";
   parent: "/window.buttons/table/frame_toggle";
}

widget
{  
   type: "toggle_button";
   name: "/window.buttons/table/frame/vbox/normal_tbutton";
   parent: "/window.buttons/table/frame_toggle/vbox";
   label: "Normal buttom";
}

widget
{  
   type: "toggle_button";
   name: "/window.buttons/table/frame_toggle/vbox/empty_tbutton";
   parent: "/window.buttons/table/frame_toggle/vbox";
}

widget
{  
   type: "toggle_button";
   name: "/window.buttons/table/frame_toggle/vbox/disabled_tbutton1";
   parent: "/window.buttons/table/frame_toggle/vbox";
   label: "Disabled toggle button 1";
   disabled: 1;
}

widget
{  
   type: "toggle_button";
   name: "/window.buttons/table/frame_toggle/vbox/disabled_tbutton2";
   parent: "/window.buttons/table/frame_toggle/vbox";
   label: "Disabled toggle button 2";
   disabled: 1;
   active: 1;
}

widget
{
   type: "frame";
   name: "/window.buttons/table/frame_check";
   parent: "/window.buttons/table";
   label: "Check Buttons";
   packing
   {
      left_attach: 0;
      right_attach: 0;
      top_attach: 1;
      bottom_attach: 1;
   }
}

widget
{
   type: "vbox";
   name: "/window.buttons/table/frame_check/vbox";
   parent: "/window.buttons/table/frame_check";
}

widget
{  
   type: "check_button";
   name: "/window.buttons/table/frame/vbox/normal_tbutton";
   parent: "/window.buttons/table/frame_check/vbox";
   label: "Check buttom";
}

widget
{  
   type: "check_button";
   name: "/window.buttons/table/frame_check/vbox/empty_tbutton";
   parent: "/window.buttons/table/frame_check/vbox";
}

widget
{  
   type: "check_button";
   name: "/window.buttons/table/frame_check/vbox/disabled_tbutton1";
   parent: "/window.buttons/table/frame_check/vbox";
   label: "Disabled check button 1";
   disabled: 1;
}

widget
{  
   type: "check_button";
   name: "/window.buttons/table/frame_check/vbox/disabled_tbutton2";
   parent: "/window.buttons/table/frame_check/vbox";
   label: "Disabled check button 2";
   disabled: 1;
   active: 1;
}



















