group {
    name: "panel/image/right";
    parts {
    	part {
			name: "object.panel.image.photo";
			type: EXTERNAL;
			source: "elm/photocam";
			description {
				state: "default" 0.0;
				rel1.relative: 0.0 0.0;
				rel2.to_y: "object.panel.image.slideshow";
				rel2.relative: 1.0 0.0;
				rel2.offset: 0 -10;
			}
		}
    	part {
			name: "object.panel.image.slideshow";
			type: SWALLOW;
			description {
				state: "default" 0.0;
				rel1.relative: 0.0 1.0;
				rel2.relative: 0.0 1.0;
				align: 0.0 1.0;
				fixed: 1 1;
			}
		}
		part {
			name: "object.panel.image.1_1";
			type: EXTERNAL;
			source: "elm/button";
			description {
				state: "default" 0.0;
				rel1.to_x: "object.panel.image.slideshow";
				rel1.relative: 1.0 1.0;
				rel2.to_x: "object.panel.image.slideshow";
				rel2.relative: 1.0 1.0;
				align: 0.0 1.0;
				fixed: 1 1;
				params {
					string: "label" "zoom 1:1";
				}
			}
		}
		part {
			name: "object.panel.image.fit";
			type: EXTERNAL;
			source: "elm/button";
			description {
				state: "default" 0.0;
				rel1.to_x: "object.panel.image.1_1";
				rel1.relative: 1.0 1.0;
				rel2.to_x: "object.panel.image.1_1";
				rel2.relative: 1.0 1.0;
				align: 0.0 1.0;
				fixed: 1 1;
				params {
					string: "label" "Fit";
				}
			}
		}
		part {
			name: "object.panel.image.fill";
			type: EXTERNAL;
			source: "elm/button";
			description {
				state: "default" 0.0;
				rel1.to_x: "object.panel.image.fit";
				rel1.relative: 1.0 1.0;
				rel2.to_x: "object.panel.image.fit";
				rel2.relative: 1.0 1.0;
				align: 0.0 1.0;
				fixed: 1 1;
				params {
					string: "label" "Fill";
				}
			}
		}
    	part {
			name: "object.panel.image.zoom";
			type: EXTERNAL;
			source: "elm/slider";
			description {
				state: "default" 0.0;
				min: 80 0;
				rel1.to_x: "object.panel.image.fill";
				rel1.relative: 1.0 1.0;
				rel2.relative: 1.0 1.0;
				align: 1.0 1.0;
				fixed: 1 1;
				params {
					string: "label" "Zoom";
					string: "format" "%3.0f";
					double: "min" "1";
					double: "max" "10";
					double: "value" "1";
					string: "unit format" "%4.0f";
				}
			}
		}
    }
}

