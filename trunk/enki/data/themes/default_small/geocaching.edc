#define header_item(group_name, object_name, lbl) 	\
group {												\
	name: group_name;								\
	parts {											\
		part {										\
			name: "label";							\
			type: EXTERNAL;							\
			source: "elm/label";					\
			description {							\
				state: "default" 0.0;				\
				fixed: 1 1;							\
				align: 0.0 0.5;						\
				params {							\
					string: "label" lbl;			\
				}									\
			}										\
		}											\
		part {										\
			name: object_name;						\
			type: EXTERNAL;							\
			source: "elm/label";					\
			description {							\
				state: "default" 0.0;				\
				fixed: 1 1;							\
				align: 0.0 0.5;						\
				rel1.to_x: "label"; 				\
				rel1.relative: 1.0 0.0;				\
			}										\
		}											\
	} 												\
}										

#define header_item_stars(group_name, lbl) 			\
group {												\
	name: group_name;								\
	parts {											\
		part {										\
			name: "label";							\
			type: EXTERNAL;							\
			source: "elm/label";					\
			description {							\
				state: "default" 0.0;				\
				fixed: 1 1;							\
				align: 0.0 0.5;						\
				params {							\
					string: "label" lbl;			\
				}									\
			}										\
		}											\
		stars("label"); 							\
	}												\
}

