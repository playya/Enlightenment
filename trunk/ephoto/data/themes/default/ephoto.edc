            description { state: "default" 0.0;
               visible: 0;
               color: 255 255 255 0;
               rel1.offset: -3 -3;
               rel2.offset: 2 2;
               image { normal: "bt_sm_base1.png";
                  border: 6 6 6 6;
               }
               image.middle: SOLID;
            }
            description { state: "selected" 0.0;
               inherit: "default" 0.0;
