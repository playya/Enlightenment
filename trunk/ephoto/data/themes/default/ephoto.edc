						to: "image";
						offset: -18 -18;
					}
					rel2 
					{
						to: "image";
						offset: 17 17;
					}
					image 
					{
						normal: "thumb_shadow.png";
						border: 17 17 17 17;
						 middle: NONE;
					}
				}
			}
			part
			{
				name: "border";
				type: RECT;
				mouse_events: 0;
				description
				{
					state: "default" 0.0;
					rel1
					{
						to: "border-shadow";
						offset: 16 16;
					}
					rel2
					{
						to: "border-shadow";
						offset: -15 -15;
					}
				}
			}
			part
			{
				name: "image";
				type: SWALLOW;
				mouse_events: 0;
				description
				{
					state: "default" 0.0;
					aspect_preference: BOTH;
					aspect: 1.0 1.0;
					rel1.offset: 0 8;
					rel2.offset: -1 -2;
				}
			}
