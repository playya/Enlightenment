		{
			part
			{
				name: "event";
				type: RECT;
				repeat_events: 1;
				description
				{
					state: "default" 0.0;
					color: 0 0 0 0;
				}
			}
			part
			{
				name: "border-shadow";
				type: IMAGE;
				mouse_events: 0;
				description
				{
					state: "default" 0.0;
					rel1 
