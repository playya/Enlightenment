#: src/bin/about.c:97
msgid "Common shortcuts"
msgstr "Raccourcis"

#: src/bin/about.c:114
msgid ""
"<b>control-n:</b> create a new note<br><b>control-q:</b> quit "
"application<br><b>control-c:</b> copy selected text<br><b>control-x:</b> cut "
"selected text<br><b>control-v:</b> paste text from clipboard<br>"
msgstr ""
"<b>control-n:</b> créer une nouvelle note<br><b>control-q:</b> quitter "
"l'application<br><b>control-c:</b> copier le texte sélectionné<br><b>control-"
"x:</b> couper le texte sélectionné<br><b>control-v:</b> coller le texte du "
"presse-papiers<br>"

